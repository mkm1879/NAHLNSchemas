<?xml version="1.0" encoding="UTF-8"?>
<!-- ******* Version 0_0_2 of the NAHLN (External) Base Order Schema ******* -->
<!--
This is the basic schema for all NAHLN laboratory order messages.
It includes all fields that we anticipate needing in any of the disease
program or surveillance testing.  It requires only the minimum needed
for all types of testing.  

As much as possible the components are taken from the result message
so that basic definitions can be reused.
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:hl7="urn:hl7-org:v2xml">
	<xsd:simpleType name="OID">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\s)*(([1-9][0-9]*)|(0))(\.(([1-9][0-9]*)|(0)))+(\s)*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="COUNTRYCODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Area Code -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-9]{1}\d*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AREACODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Area Code -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[2-9]{1}\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PHONENUMBER">
		<!-- 11/30/2007 Slinke Added Expression to validate Phone Num -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EXTENSION">
		<!-- 11/30/2007 Slinke Added Expression to validate Extension -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{1,5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ISO">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="ISO"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UNITS">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ISO+"/>
			<xsd:enumeration value="ANS+"/>
			<xsd:enumeration value="UCUM"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LN">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="LN"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LocalCode">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="L"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LNorLocal">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LN"/>
			<xsd:enumeration value="L"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Local">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="L"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NAISID">
		<xsd:restriction base="xsd:string">
			<!--PIN OID-->
			<xsd:enumeration value="2.16.840.1.113883.3.5.6.1.1"/>
			<!-- NPPN OID -->
			<xsd:enumeration value="2.16.840.1.113883.3.5.6.1.4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CondDateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StateCode">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AK"/>
			<xsd:enumeration value="AL"/>
			<xsd:enumeration value="AR"/>
			<xsd:enumeration value="AS"/>
			<xsd:enumeration value="AZ"/>
			<xsd:enumeration value="CA"/>
			<xsd:enumeration value="CO"/>
			<xsd:enumeration value="CT"/>
			<xsd:enumeration value="DC"/>
			<xsd:enumeration value="DE"/>
			<xsd:enumeration value="FL"/>
			<xsd:enumeration value="FM"/>
			<xsd:enumeration value="GA"/>
			<xsd:enumeration value="GB"/>
			<xsd:enumeration value="GU"/>
			<xsd:enumeration value="HI"/>
			<xsd:enumeration value="IA"/>
			<xsd:enumeration value="ID"/>
			<xsd:enumeration value="IL"/>
			<xsd:enumeration value="IN"/>
			<xsd:enumeration value="KS"/>
			<xsd:enumeration value="KY"/>
			<xsd:enumeration value="LA"/>
			<xsd:enumeration value="MA"/>
			<xsd:enumeration value="MD"/>
			<xsd:enumeration value="ME"/>
			<xsd:enumeration value="MH"/>
			<xsd:enumeration value="MI"/>
			<xsd:enumeration value="MN"/>
			<xsd:enumeration value="MO"/>
			<xsd:enumeration value="MP"/>
			<xsd:enumeration value="MS"/>
			<xsd:enumeration value="MT"/>
			<xsd:enumeration value="NC"/>
			<xsd:enumeration value="ND"/>
			<xsd:enumeration value="NE"/>
			<xsd:enumeration value="NH"/>
			<xsd:enumeration value="NJ"/>
			<xsd:enumeration value="NM"/>
			<xsd:enumeration value="NV"/>
			<xsd:enumeration value="NY"/>
			<xsd:enumeration value="OH"/>
			<xsd:enumeration value="OK"/>
			<xsd:enumeration value="OR"/>
			<xsd:enumeration value="PA"/>
			<xsd:enumeration value="PR"/>
			<xsd:enumeration value="PW"/>
			<xsd:enumeration value="PZ"/>
			<xsd:enumeration value="RI"/>
			<xsd:enumeration value="SC"/>
			<xsd:enumeration value="SD"/>
			<xsd:enumeration value="TN"/>
			<xsd:enumeration value="TX"/>
			<xsd:enumeration value="UM"/>
			<xsd:enumeration value="UT"/>
			<xsd:enumeration value="VA"/>
			<xsd:enumeration value="VI"/>
			<xsd:enumeration value="VT"/>
			<xsd:enumeration value="WA"/>
			<xsd:enumeration value="WI"/>
			<xsd:enumeration value="WK"/>
			<xsd:enumeration value="WV"/>
			<xsd:enumeration value="WY"/>
			<xsd:enumeration value="ZK"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="YN">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="N"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FT">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="IDType">
		<xsd:restriction base="xsd:string">
			<!-- Anonymous identifier -->
			<xsd:enumeration value="ANON"/>
			<!-- Band -->
			<xsd:enumeration value="B"/>
			<!-- Band, Leg  -->
			<xsd:enumeration value="BL"/>
			<!-- Birth registry number -->
			<xsd:enumeration value="BR"/>
			<!-- Brand -->
			<xsd:enumeration value="BRD"/>
			<!-- Brand, Freeze  -->
			<xsd:enumeration value="BRF"/>
			<!-- Brand, Hot Iron  -->
			<xsd:enumeration value="BRH"/>
			<!-- Breed Registry Number -->
			<xsd:enumeration value="BRN"/>
			<!-- Collection Site Identification number -->
			<xsd:enumeration value="CSID"/>
			<!-- Facility ID -->
			<xsd:enumeration value="FI"/>
			<!-- Guarantor internal identifier -->
			<xsd:enumeration value="GI"/>
			<!-- Guarantor external  identifier -->
			<xsd:enumeration value="GN"/>
			<!-- Group Identifier -->
			<xsd:enumeration value="GRID"/>
			<!-- Local Registry ID -->
			<xsd:enumeration value="LR"/>
			<!-- Microchip Number -->
			<xsd:enumeration value="MCN"/>
			<!-- Medical record number -->
			<xsd:enumeration value="MR"/>
			<!-- Temporary Medical Record Number -->
			<xsd:enumeration value="MRT"/>
			<!-- Notch -->
			<xsd:enumeration value="N"/>
			<!-- Neck Chain Identifier -->
			<xsd:enumeration value="NCHN"/>
			<!-- Notch, Ear  -->
			<xsd:enumeration value="NE"/>
			<!-- Notch, Fin  -->
			<xsd:enumeration value="NF"/>
			<!-- Living Subject Enterprise Number -->
			<xsd:enumeration value="PE"/>
			<!-- Temporary Living Subject Number -->
			<xsd:enumeration value="PNT"/>
			<!-- Patient external identifier -->
			<xsd:enumeration value="PT"/>
			<!-- QA number -->
			<xsd:enumeration value="QA"/>
			<!-- Regional Registry Number -->
			<xsd:enumeration value="RRI"/>
			<!-- State Registry Number -->
			<xsd:enumeration value="SR"/>
			<!-- Tag -->
			<xsd:enumeration value="TAG"/>
			<!-- Tag, Back -->
			<xsd:enumeration value="TAGB"/>
			<!-- Tag, FSIS Condemnation  -->
			<xsd:enumeration value="TAGC"/>
			<!-- Tag, Ear -->
			<xsd:enumeration value="TAGE"/>
			<!-- Tag, Fin  -->
			<xsd:enumeration value="TAGF"/>
			<!-- Tag, Neck  -->
			<xsd:enumeration value="TAGN"/>
			<!-- Tag, Official USDA Bangle  -->
			<xsd:enumeration value="TAGOB "/>
			<!-- Tag, Owner Ear -->
			<xsd:enumeration value="TAGOE"/>
			<!-- Tag, Official USDA Silver  -->
			<xsd:enumeration value="TAGOS"/>
			<!-- Tag, Official USDA Vaccination  -->
			<xsd:enumeration value="TAGOV"/>
			<!-- Tag, Tail -->
			<xsd:enumeration value="TAGT"/>
			<!-- Tag, Wing  -->
			<xsd:enumeration value="TAGW"/>
			<!-- Tattoo -->
			<xsd:enumeration value="TAT"/>
			<!-- Tattoo, Body  -->
			<xsd:enumeration value="TATB"/>
			<!-- Tattoo, Ear  -->
			<xsd:enumeration value="TATE"/>
			<!-- Tattoo, Lip  -->
			<xsd:enumeration value="TATL"/>
			<!-- Unspecified identifier -->
			<xsd:enumeration value="U"/>
