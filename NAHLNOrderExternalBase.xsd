	<xsd:simpleType name="SCT">
		<!-- Snomed Coding System -->
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SCT"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="nonNullString">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="OPL_O37">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Header consists of MSH for Routing, SFT for admin, ROL and GT1 for accession parties -->
				<xsd:element ref="MSH"/>
				<!-- Optionally followed by one or more software identifiers for systems generating
					or formatting the order -->
				<xsd:element ref="SFT" minOccurs="0" maxOccurs="unbounded"/>
				<!-- And one or more roles for Submitter plus, depending on program, collector, premises, etc. -->
				<!-- Most laboratories require a submitter so we require the SUB instance. -->
				<xsd:element ref="NTE" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ROL" minOccurs="1" maxOccurs="unbounded"/>
				<!-- Information on who will be paying the bill.  "Bill-to" in many LIMS -->
				<!-- May be restricted to single occurrance by local convention -->
				<!-- If multiple, division to be determined outside of messaging, by local convention, etc -->
				<xsd:element ref="GT1" minOccurs="1" maxOccurs="unbounded"/>
				<!-- Followed by one or more Order groups -->
				<xsd:element ref="OPL_O37.ORDER" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- The MSH segment is almost all either NAHLN or Lab Constants
		Insert date/time of message and message control id at run time.
	 -->
	<xsd:element name="MSH">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NAHLN Constant | -->
				<xsd:element name="MSH.1" minOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="nonNullString">
							<xsd:enumeration value="|"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- NAHLN Constant ^~\&amp; -->
				<xsd:element name="MSH.2" minOccurs="1">
					<xsd:simpleType>
						<!--4/30/2008 Slinke.  Removed enum value due to rhapsody bug-->
						<xsd:restriction base="nonNullString"/>
					</xsd:simpleType>
				</xsd:element>
				<!-- Sending application -->
				<xsd:element name="MSH.3" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Lab Constant Application Abbrev -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- OID of system issuing application ID -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Sending facility -->
				<xsd:element name="MSH.4">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Lab Constant Lab PremID or Non-producer participant ID -->
							<!-- How do we populate with Ordering systems without a PIN? -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- Lab Constant 2.16.840.1.113883.3.5.6.1.[1 or 4] (PIN or NPPN) -->
							<!-- 04/29/2008 Slinke. Changed from type OID to enumerated list. -->
							<xsd:element name="HD.2" type="NAISID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Receiving application -->
				<xsd:element name="MSH.5" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- LIMS Application Abbrev -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- Lab OID -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Receiving facility -->
				<xsd:element name="MSH.6" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- PIN of laboratory prefered or other by local agreement -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- 2.16.840.1.113883.3.5.6.1.1 for NAHLN or OID of issuing authority for others -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- For the NAHLN this will be restricted to PIN
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										PIN OID
										<xsd:enumeration value="2.16.840.1.113883.3.5.6.1.1"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element> -->
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Date/time of message as YYYYMMDDHHMMSS-ZZZZ -->
				<xsd:element name="MSH.7" type="DateTime" minOccurs="1"/>
				<!-- HL7 Message type -->
				<xsd:element name="MSH.9" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant OPU -->
							<xsd:element name="MSG.1" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="OPL"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN Constant R25 -->
							<xsd:element name="MSG.2" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="O37"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN Constant OPU_R25  -->
							<xsd:element name="MSG.3" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="OPL_O37"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Insert sender's identifier for message used to match up errors, etc.  -->
				<xsd:element name="MSH.10" type="nonNullString"/>
				<!-- Processing ID -->
				<xsd:element name="MSH.11">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant P, Production or D, Debugging or T, Training -->
							<xsd:element name="PT.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Production -->
										<xsd:enumeration value="P"/>
										<!-- Debugging -->
										<xsd:enumeration value="D"/>
										<!-- Training -->
										<xsd:enumeration value="T"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Version ID -->
				<xsd:element name="MSH.12">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant 2.6 -->
							<xsd:element name="VID.1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<!-- Add values as we support 2.7, etc. -->
										<xsd:enumeration value="2.6"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Message profile -->
				<xsd:element name="MSH.21" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Profile ID -->
							<xsd:element name="EI.1" minOccurs="1" type="nonNullString"/>
							<!-- NAHLN Constant OID for NAHLN Profile IDs 2.16.840.1.113883.3.5.9 -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END MSH -->

	<!-- Software Segment holds information about software that created or
		formatted the order.  Does not usually include software that only
		transmitted fully populated messages since this software does not
		read or write the message bocy. -->
	<xsd:element name="SFT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="SFT.1" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Software Vendor Name -->
							<xsd:element name="XON.1" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Version or release number -->
				<xsd:element name="SFT.2" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<!-- Version Product Name -->
				<xsd:element name="SFT.3" type="xsd:string" minOccurs="1" maxOccurs="1"/>
				<!-- Software Binary ID e.g. Checksum, MD5, etc. -->
				<xsd:element name="SFT.4" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Notes segments are extremely dangerous.  The information contained in 
		an NTE segment MUST NOT add or subtract from the machine-processable
		information contained in the rest of the message. The ONLY legitimate 
	    use of an NTE is to communicate to the human being who will be on the
	    system receiving the message, something that is not otherwise carried 
	    in the message or needed for a complete record of events.  
	
		Used here primarily to allow transmission of case history not included
	    elsewhere. 
	
		The most likely usage is to include ONLY the NTE.3 comment itself in 
	    a format that will make sense on its own. -->
	<xsd:element name="NTE">
		<xsd:complexType>
			<xsd:sequence>
				<!-- The comment itself in text.  For veterinary use, only plain text
					 is supported.  The Formatted Text datatype is retained only for 
					 compatibility with the base standard. -->
				<xsd:element name="NTE.3" type="FT" minOccurs="1" maxOccurs="unbounded"/>
				<!-- Comment Type -->
				<xsd:element name="NTE.4" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Codes and coding systems to be determined by 
							     local agreement.  TODO specify a generic patient 
							     history LN code -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Text -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Code System -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alt Text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code System -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Entered by.  Rarely used but available if it is important to 
					note who entered this comment. -->
				<xsd:element name="NTE.5" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Person identifier. Splitting hairs between ROL.2 and here.
								This identifies the person generally, not just in this role for this order.
								This would be the right place for any general person ID in the system-->
							<xsd:element name="XCN.1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="XCN.2">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last Name ("Not Applicable" for Premises) -->
										<xsd:element name="FN.1">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
													<xsd:maxLength value="30"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name -->
							<xsd:element name="XCN.3" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name -->
							<xsd:element name="XCN.4" type="xsd:string" minOccurs="0"/>
							<!-- Name Suffix -->
							<xsd:element name="XCN.5" type="xsd:string" minOccurs="0"/>
							<!-- Assigning Authority (of identifier in XCN.1)  -->
							<xsd:element name="XCN.9" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- OID of assigner of ID in XCN.1 -->
										<xsd:element name="HD.2" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="HD.3" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Roles used for Submitter, Collector, Premises, etc. -->
	<xsd:element name="ROL">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Role instance ID.  Identifier qualifying this person to participate in this role. -->
				<xsd:element name="ROL.1" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Identifier, participant number, etc. -->
							<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
							<!-- OID of identifier system such as collection program -->
							<xsd:element name="EI.3" type="OID" minOccurs="1"/>
							<!-- NAHLN Constant "ISO" -->
							<xsd:element name="EI.4" type="ISO" minOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Action code.  "AD" for initial order, "UC" for all other messages. -->
				<xsd:element name="ROL.2">
					<xsd:simpleType>
						<xsd:restriction base="nonNullString">
							<xsd:enumeration value="AD"/>
							<xsd:enumeration value="UC"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- Role -->
				<xsd:element name="ROL.3">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN code for Role "SUB", "COL", "PREM", etc. -->
							<xsd:element name="CWE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Submitter -->
										<xsd:enumeration value="SUB"/>
										<!-- Collector -->
										<xsd:enumeration value="COL"/>
										<!-- Source Premises -->
										<xsd:enumeration value="PREM"/>
										<!-- Attending Veterinarian -->
										<xsd:enumeration value="AT"/>
										<!-- Parent Company -->
										<xsd:enumeration value="PAR"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Spelled out role "Submitting Party", "Specimen Collector", "Source Premises", etc. -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" Local code.  Local here means local to the NAHLN
							     and related user groups -->
							<xsd:element name="CWE.3" type="Local"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Role Person.  Who is the submitter, collector, etc.  Doesn't make sense for Premises but required placeholder.
				     The issue of non-humans (organizations, premises) in ROL is being addressed at HL7  -->
				<xsd:element name="ROL.4" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Person identifier. Splitting hairs between ROL.2 and here.
							     This identifies the person generally, not just in this role for this order.
							     This would be the right place for any general person ID in the system-->
							<xsd:element name="XCN.1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="XCN.2">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last Name ("Not Applicable" for Premises) -->
										<xsd:element name="FN.1">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
													<xsd:maxLength value="30"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name -->
							<xsd:element name="XCN.3" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name -->
							<xsd:element name="XCN.4" type="xsd:string" minOccurs="0"/>
							<!-- Name Suffix -->
							<xsd:element name="XCN.5" type="xsd:string" minOccurs="0"/>
							<!-- Assigning Authority (of identifier in XCN.1)  -->
							<xsd:element name="XCN.9" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- OID of assigner of ID in XCN.1 -->
										<xsd:element name="HD.2" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="HD.3" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Address -->
				<xsd:element name="ROL.11" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XAD.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="nonNullString"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="nonNullString"/>
							<!-- GPS Coordinates, etc. -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County code 3 or 5 digit FIPS county code -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Phone -->
				<xsd:element name="ROL.12" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN, PRN, EMR, NET, etc. -->
							<xsd:element name="XTN.2">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Answering Service Number -->
										<xsd:enumeration value="ASN"/>
										<!-- Beeper Number -->
										<xsd:enumeration value="BPN"/>
										<!-- Emergency Number -->
										<xsd:enumeration value="EMR"/>
										<!-- Network (email) Address -->
										<xsd:enumeration value="NET"/>
										<!-- Other Residence Number -->
										<xsd:enumeration value="ORN"/>
										<!-- Primary Residence Number -->
										<xsd:enumeration value="PRN"/>
										<!-- Vacation Home Number -->
										<xsd:enumeration value="VHN"/>
										<!-- Work Number -->
										<xsd:enumeration value="WPN"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Equipment type: PH Phone, FX Fax, Internet Email -->
							<xsd:element name="XTN.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Beeper -->
										<xsd:enumeration value="BP"/>
										<!-- Cellular Phone -->
										<xsd:enumeration value="CP"/>
										<!-- Fax -->
										<xsd:enumeration value="FX"/>
										<!-- Internet Address, use only when XTN.2 is NET -->
										<xsd:enumeration value="Internet"/>
										<!-- Modem -->
										<xsd:enumeration value="MD"/>
										<!-- Telephone -->
										<xsd:enumeration value="PH"/>
										<!-- Telecommunications Device for the Deaf -->
										<xsd:enumeration value="TDD"/>
										<!-- Teletypewriter -->
										<xsd:enumeration value="TTY"/>
										<!-- X.400 email address, use only when XTN.2 is NET -->
										<xsd:enumeration value="X.400"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="COUNTRYCODE" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="AREACODE" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="PHONENUMBER" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="EXTENSION" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Person location Used for premises ID -->
				<xsd:element name="ROL.13" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Location Type uses premises type codes from Animal Disease Traceability (formerly NAIS)
								 "P" Production Unit is the usual for a farm, etc. -->
							<xsd:element name="PL.6" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Clinic -->
										<xsd:enumeration value="C"/>
										<!-- Exhibition -->
										<xsd:enumeration value="E"/>
										<!-- Laboratory -->
										<xsd:enumeration value="L"/>
										<!-- Market/Collection Point -->
										<xsd:enumeration value="M"/>
										<!-- Production Unit -->
										<xsd:enumeration value="P"/>
										<!-- Port of Entry -->
										<xsd:enumeration value="B"/>
										<!-- Quarantine Facility -->
										<xsd:enumeration value="Q"/>
										<!-- Slaughter Plant -->
										<xsd:enumeration value="S"/>
										<!-- Tagging Site -->
										<xsd:enumeration value="T"/>
										<!-- Rendering -->
										<xsd:enumeration value="R"/>
										<!-- Non-producer Participant Not applicable Here-->
										<!-- <xsd:enumeration value="N"/> -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Location Description = Entity Name in ADT -->
							<xsd:element name="PL.9" type="xsd:string" minOccurs="0"/>
							<!-- Location Identifier -->
							<xsd:element name="PL.10" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Premises ID -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- PremID OID 2.16.840.1.113883.3.5.6.1.1
										     or alternate OID for State-issued location IDs -->
										<xsd:element name="EI.3" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- GT1 Holds the Bill-to person or organization -->
	<xsd:element name="GT1">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Simple sequence number normally 1 -->
				<xsd:element name="GT1.1" minOccurs="1" maxOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:int">
							<xsd:enumeration value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- Gaurantor Number (account number) If provided, full address info is optional -->
				<xsd:element name="GT1.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CX.1" type="nonNullString" minOccurs="1" maxOccurs="1"/>
							<xsd:element name="CX.4">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Unformatted assigner if no OID assigned, non-NAHLN use only -->
										<xsd:element name="HD.1" type="xsd:string" minOccurs="0" maxOccurs="1"/>
										<!-- OID of assigner of ID in XCN.1 -->
										<xsd:element name="HD.2" type="OID" minOccurs="0" maxOccurs="1"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="HD.3" type="ISO" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Guarantor name -->
				<xsd:element name="GT1.3">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XPN.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last name of guarantor.  Send "" if GT is an organization -->
										<xsd:element name="FN.1" type="nonNullString"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name of guarantor -->
							<xsd:element name="XPN.2" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name of guarantor -->
							<xsd:element name="XPN.3" type="xsd:string" minOccurs="0"/>
							<!-- Name suffix (sr. jr. etc.) of guarantor -->
							<xsd:element name="XPN.4" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Guarantor address -->
				<xsd:element name="GT1.5" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XAD.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="nonNullString"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="nonNullString"/>
							<!-- Lat/Long etc. -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Guarantor Home Phone -->
				<xsd:element name="GT1.6" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN, PRN, EMR, NET, etc. -->
							<xsd:element name="XTN.2">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Answering Service Number -->
										<xsd:enumeration value="ASN"/>
										<!-- Beeper Number -->
										<xsd:enumeration value="BPN"/>
										<!-- Emergency Number -->
										<xsd:enumeration value="EMR"/>
										<!-- Network (email) Address -->
										<xsd:enumeration value="NET"/>
										<!-- Other Residence Number -->
										<xsd:enumeration value="ORN"/>
										<!-- Primary Residence Number -->
										<xsd:enumeration value="PRN"/>
										<!-- Vacation Home Number -->
										<xsd:enumeration value="VHN"/>
										<!-- Work Number -->
										<xsd:enumeration value="WPN"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Equipment type: PH Phone, FX Fax, Internet Email -->
							<xsd:element name="XTN.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Beeper -->
										<xsd:enumeration value="BP"/>
										<!-- Cellular Phone -->
										<xsd:enumeration value="CP"/>
										<!-- Fax -->
										<xsd:enumeration value="FX"/>
										<!-- Internet Address, use only when XTN.2 is NET -->
										<xsd:enumeration value="Internet"/>
										<!-- Modem -->
										<xsd:enumeration value="MD"/>
										<!-- Telephone -->
										<xsd:enumeration value="PH"/>
										<!-- Telecommunications Device for the Deaf -->
										<xsd:enumeration value="TDD"/>
										<!-- Teletypewriter -->
										<xsd:enumeration value="TTY"/>
										<!-- X.400 email address, use only when XTN.2 is NET -->
										<xsd:enumeration value="X.400"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="COUNTRYCODE" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="AREACODE" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="PHONENUMBER" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="EXTENSION" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Guarantor Work Phone -->
				<xsd:element name="GT1.7" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN, PRN, EMR, NET, etc. -->
							<xsd:element name="XTN.2">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Answering Service Number -->
										<xsd:enumeration value="ASN"/>
										<!-- Beeper Number -->
										<xsd:enumeration value="BPN"/>
										<!-- Emergency Number -->
										<xsd:enumeration value="EMR"/>
										<!-- Network (email) Address -->
										<xsd:enumeration value="NET"/>
										<!-- Other Residence Number -->
										<xsd:enumeration value="ORN"/>
										<!-- Primary Residence Number -->
										<xsd:enumeration value="PRN"/>
										<!-- Vacation Home Number -->
										<xsd:enumeration value="VHN"/>
										<!-- Work Number -->
										<xsd:enumeration value="WPN"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Equipment type: PH Phone, FX Fax, Internet Email -->
							<xsd:element name="XTN.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Beeper -->
										<xsd:enumeration value="BP"/>
										<!-- Cellular Phone -->
										<xsd:enumeration value="CP"/>
										<!-- Fax -->
										<xsd:enumeration value="FX"/>
										<!-- Internet Address, use only when XTN.2 is NET -->
										<xsd:enumeration value="Internet"/>
										<!-- Modem -->
										<xsd:enumeration value="MD"/>
										<!-- Telephone -->
										<xsd:enumeration value="PH"/>
										<!-- Telecommunications Device for the Deaf -->
										<xsd:enumeration value="TDD"/>
										<!-- Teletypewriter -->
										<xsd:enumeration value="TTY"/>
										<!-- X.400 email address, use only when XTN.2 is NET -->
										<xsd:enumeration value="X.400"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="COUNTRYCODE" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="AREACODE" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="PHONENUMBER" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="EXTENSION" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Guarantor Type: Codes to be determined.  Convert to enumeration at that time -->
				<xsd:element name="GT1.10" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<!-- Organization Name -->
				<xsd:element name="GT1.21" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Business Name -->
							<xsd:element name="XON.1" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END GT11  -->

	<!-- Accession Detail Repeats for each animal or pool. -->
	<xsd:element name="OPL_O37.ORDER">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NK1 Represents the "Owner" who is the person most responsible for the animals or location being tested -->
				<xsd:element ref="NK1"  minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element ref="OPL_O37.PATIENT" minOccurs="0" maxOccurs="1"/>
				<!-- set to  in element definition below.-->
				<xsd:element ref="OPL_O37.SPECIMEN" minOccurs="1" maxOccurs="unbounded"/>
				<!-- Prior Result generally only used for confiratory testing -->
				<xsd:element ref="OPL_O37.PRIOR_RESULT" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END OPU_R25.ACCESSION_DETAIL -->

	<!-- Next of Kin: Owner -->
	<xsd:element name="NK1">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Sequence ID: NAHLN Constant "1" -->
				<xsd:element name="NK1.1" type="xsd:int"/>
				<!-- Owner Name -->
				<xsd:element name="NK1.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XPN.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last name -->
										<xsd:element name="FN.1" type="nonNullString"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name -->
							<xsd:element name="XPN.2" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name -->
							<xsd:element name="XPN.3" type="xsd:string" minOccurs="0"/>
							<!-- Name Suffix -->
							<xsd:element name="XPN.4" type="xsd:string" minOccurs="0"/>
							<!-- Name type code -->
							<xsd:element name="XPN.7" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Relationship -->
				<xsd:element name="NK1.3" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN default "OWN" -->
							<xsd:element name="CWE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Associate -->
										<xsd:enumeration value="ASC"/>
										<!-- Emergency Contact -->
										<xsd:enumeration value="EMC"/>
										<!-- Guardian -->
										<xsd:enumeration value="GRD"/>
										<!-- Manager -->
										<xsd:enumeration value="MGR"/>
										<!-- Owner -->
										<xsd:enumeration value="OWN"/>
										<!-- Trainer -->
										<xsd:enumeration value="TRA"/>
										<!-- Unknown -->
										<xsd:enumeration value="UNK"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN default "Owner" -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70063" -->
							<xsd:element name="CWE.3" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner Address -->
				<xsd:element name="NK1.4" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XAD.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString" minOccurs="1"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation Address Line 2 -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="xsd:string"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="xsd:string"/>
							<!-- Lat/Long -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner home phone(s) -->
				<xsd:element name="NK1.5" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN, PRN, EMR, NET, etc. -->
							<xsd:element name="XTN.2">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Answering Service Number -->
										<xsd:enumeration value="ASN"/>
										<!-- Beeper Number -->
										<xsd:enumeration value="BPN"/>
										<!-- Emergency Number -->
										<xsd:enumeration value="EMR"/>
										<!-- Network (email) Address -->
										<xsd:enumeration value="NET"/>
										<!-- Other Residence Number -->
										<xsd:enumeration value="ORN"/>
										<!-- Primary Residence Number -->
										<xsd:enumeration value="PRN"/>
										<!-- Vacation Home Number -->
										<xsd:enumeration value="VHN"/>
										<!-- Work Number -->
										<xsd:enumeration value="WPN"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Equipment type: PH Phone, FX Fax, Internet Email -->
							<xsd:element name="XTN.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Beeper -->
										<xsd:enumeration value="BP"/>
										<!-- Cellular Phone -->
										<xsd:enumeration value="CP"/>
										<!-- Fax -->
										<xsd:enumeration value="FX"/>
										<!-- Internet Address, use only when XTN.2 is NET -->
										<xsd:enumeration value="Internet"/>
										<!-- Modem -->
										<xsd:enumeration value="MD"/>
										<!-- Telephone -->
										<xsd:enumeration value="PH"/>
										<!-- Telecommunications Device for the Deaf -->
										<xsd:enumeration value="TDD"/>
										<!-- Teletypewriter -->
										<xsd:enumeration value="TTY"/>
										<!-- X.400 email address, use only when XTN.2 is NET -->
										<xsd:enumeration value="X.400"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="COUNTRYCODE" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="xsd:int" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="xsd:int" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="xsd:int" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner business phone(s) -->
				<xsd:element name="NK1.6" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN, PRN, EMR, NET, etc. -->
							<xsd:element name="XTN.2">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Answering Service Number -->
										<xsd:enumeration value="ASN"/>
										<!-- Beeper Number -->
										<xsd:enumeration value="BPN"/>
										<!-- Emergency Number -->
										<xsd:enumeration value="EMR"/>
										<!-- Network (email) Address -->
										<xsd:enumeration value="NET"/>
										<!-- Other Residence Number -->
										<xsd:enumeration value="ORN"/>
										<!-- Primary Residence Number -->
										<xsd:enumeration value="PRN"/>
										<!-- Vacation Home Number -->
										<xsd:enumeration value="VHN"/>
										<!-- Work Number -->
										<xsd:enumeration value="WPN"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Equipment type: PH Phone, FX Fax, Internet Email -->
							<xsd:element name="XTN.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Beeper -->
										<xsd:enumeration value="BP"/>
										<!-- Cellular Phone -->
										<xsd:enumeration value="CP"/>
										<!-- Fax -->
										<xsd:enumeration value="FX"/>
										<!-- Internet Address, use only when XTN.2 is NET -->
										<xsd:enumeration value="Internet"/>
										<!-- Modem -->
										<xsd:enumeration value="MD"/>
										<!-- Telephone -->
										<xsd:enumeration value="PH"/>
										<!-- Telecommunications Device for the Deaf -->
										<xsd:enumeration value="TDD"/>
										<!-- Teletypewriter -->
										<xsd:enumeration value="TTY"/>
										<!-- X.400 email address, use only when XTN.2 is NET -->
										<xsd:enumeration value="X.400"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="COUNTRYCODE" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="xsd:int" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="xsd:int" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="xsd:int" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner business name -->
				<xsd:element name="NK1.13" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Business Name -->
							<xsd:element name="XON.1" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner primary language -->
				<xsd:element name="NK1.20" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code See list of ISO 639 abbreviations in implementation guide
								or see http://en.wikipedia.org/wiki/List_of_ISO_639-2_codes -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Text -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Code System -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alt Text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code System -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- PATIENT  -->
	<xsd:element name="OPL_O37.PATIENT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="PID"/>
				<xsd:element name="OBX" type="PATIENT.OBX" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END PATIENT -->

	<xsd:element name="PID">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Patient Identifier(s) -->
				<xsd:element name="PID.3" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Identifier (AIN or GIN preferred) -->
							<xsd:element name="CX.1" type="nonNullString"/>
							<!-- Assigning Authority -->
							<xsd:element name="CX.4" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Either Text of Assigner here -->
										<xsd:element name="HD.1" type="xsd:string" minOccurs="0"/>
										<!-- OR OID of Assigner (preferred) here -->
										<xsd:element name="HD.2" type="OID" minOccurs="0"/>
										<!-- If above is an OID, this is "ISO" if text, omit HD.3 -->
										<xsd:element name="HD.3" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Type code for type of ID such as eartag, microchip, etc. -->
							<xsd:element name="CX.5" type="IDType" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Patient Name: required for HL7 but often not sent for NAHLN -->
				<xsd:element name="PID.5">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XPN.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Family Name "Not Provided" or "NA" in most cases or animal name if given -->
										<xsd:element name="FN.1" type="nonNullString"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Name type code from HL70200: "N" or omit in most cases, "R" for official registered name -->
							<xsd:element name="XPN.7" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Date of birth to as much precision as known YYYYMMDD -->
				<xsd:element name="PID.7" type="CondDateTime" minOccurs="0"/>
				<!-- Administrative Sex (F,S,M,C,U, or X See Implementation Guide)-->
				<xsd:element name="PID.8" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="F"/>
							<!-- Female -->
							<xsd:enumeration value="S"/>
							<!-- Neutered Female -->
							<xsd:enumeration value="M"/>
							<!-- Male -->
							<xsd:enumeration value="C"/>
							<!-- Neutered Male -->
							<xsd:enumeration value="U"/>
							<!-- Gender Unknown -->
							<xsd:enumeration value="X"/>
							<!-- Multiple Genders -->
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- Race: Used for color or markings in NAHLN -->
				<xsd:element name="PID.10" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Text -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Code System: SCT or L most likely -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Patient Address -->
				<xsd:element name="PID.11" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Street Address -->
							<xsd:element name="XAD.1" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="xsd:string" minOccurs="0"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode" minOccurs="0"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="xsd:string" minOccurs="0"/>
							<!-- Lat/Long etc. -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Date and time of death to as much precision as known YYYYMMDDHHMM-ZZZZ -->
				<xsd:element name="PID.29" type="DateTime" minOccurs="0"/>
				<!-- Patient death indicator: "Y" if death, normally omitted but my be "N" for live prior to testing -->
				<xsd:element name="PID.30" type="YN" minOccurs="0"/>
				<!-- Identity unknown indicator: "Y" for John Doe's, normally omitted but may be set to "N" -->
				<xsd:element name="PID.31" type="YN" minOccurs="0"/>
				<!-- Taxonomic classification -->
				<xsd:element name="PID.35" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED Code -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED Preferred Name -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alt Code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alt Text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code System -->
							<xsd:element name="CWE.6" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Local Code -->
										<xsd:enumeration value="L"/>
										<!-- NAIS Species Code -->
										<xsd:enumeration value="NAIS"/>
										<!-- Wildlife Services Species Code -->
										<xsd:enumeration value="WS"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Strain -->
				<xsd:element name="PID.37" type="xsd:string" minOccurs="0"/>
				<!-- Production Class -->
				<xsd:element name="PID.38" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Production Type Code -->
							<xsd:element name="CWE.1" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Breeding/genetic shock -->
										<xsd:enumeration value="BR"/>
										<!-- Companion -->
										<xsd:enumeration value="CP"/>
										<!-- Dairy -->
										<xsd:enumeration value="DA"/>
										<!-- Draft -->
										<xsd:enumeration value="DR"/>
										<!-- Dual Purpose -->
										<xsd:enumeration value="DU"/>
										<!-- Exhibition -->
										<xsd:enumeration value="EX"/>
										<!-- Fiber -->
										<xsd:enumeration value="FB"/>
										<!-- Layer, including multiplier flocks -->
										<xsd:enumeration value="LY"/>
										<!-- Multiple Purpose -->
										<xsd:enumeration value="MP"/>
										<!-- Meat -->
										<xsd:enumeration value="MT"/>
										<!-- Not Applicable -->
										<xsd:enumeration value="NA"/>
										<!-- Non-Physical Product Production -->
										<xsd:enumeration value="NP"/>
										<!-- Ornamental -->
										<xsd:enumeration value="OR"/>
										<!-- Other -->
										<xsd:enumeration value="OT"/>
										<!-- Physical Product Production -->
										<xsd:enumeration value="PP"/>
										<!-- Pleasure -->
										<xsd:enumeration value="PL"/>
										<!-- Racing -->
										<xsd:enumeration value="RA"/>
										<!-- Show -->
										<xsd:enumeration value="SH"/>
										<!-- Service -->
										<xsd:enumeration value="SV"/>
										<!-- Unknown -->
										<xsd:enumeration value="U"/>
										<!-- Wildlife -->
										<xsd:enumeration value="WI"/>
										<!-- Zoo -->
										<xsd:enumeration value="Z"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Text -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant  "HL70429" -->
							<xsd:element name="CWE.3" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70429"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Alt Code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alt Text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code System -->
							<xsd:element name="CWE.6" type="Local" minOccurs="0"/>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>


	<xsd:complexType name="PATIENT.OBX">
		<xsd:sequence>
			<!-- Currently NAHLN constant "NM" but schema supports other value types FUO -->
			<xsd:element name="OBX.2">
				<!-- Result type code CWE, NM, SN, ST -->
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<!-- Coded With Exceptions -->
						<xsd:enumeration value="CWE"/>
						<!-- Numeric -->
						<xsd:enumeration value="NM"/>
						<!-- Structured Numeric -->
						<xsd:enumeration value="SN"/>
						<!-- String -->
						<xsd:enumeration value="ST"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Observation Identifier -->
			<xsd:element name="OBX.3">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Loinc code of result -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Loinc short name -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "LN" -->
						<xsd:element name="CWE.3" type="LN"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Observation sub id -->
			<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
			<!-- Observation Value -->
			<xsd:element name="OBX.5">
				<!-- Our definition of ANY because this may support various types of observations about the patient -->
				<!-- mixed="true" allows for ST and NM types -->
				<xsd:complexType mixed="true">
					<xsd:sequence>
						<!-- Comparator: =, <, <=, >, >= -->
						<xsd:element name="SN.1" type="xsd:string" minOccurs="0"/>
						<!-- Number 1 -->
						<xsd:element name="SN.2" type="xsd:string" minOccurs="0"/>
						<!-- Separator: :, -, etc.-->
						<xsd:element name="SN.3" type="xsd:string" minOccurs="0"/>
						<!-- Number 2 -->
						<xsd:element name="SN.4" type="xsd:string" minOccurs="0"/>
						<!-- Code -->
						<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
						<!-- Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- Code System -->
						<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Units if applicable -->
			<xsd:element name="OBX.6" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Units Code -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Units Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN constant the "ISO+" -->
						<xsd:element name="CWE.3" type="UNITS"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Interpretation of result "Abornomal flags" prior to version 2.7 -->
			<xsd:element name="OBX.8" type="xsd:string" minOccurs="0"/>
			<!-- NAHLN Constant "F" Only final results messaged -->
			<xsd:element name="OBX.11" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="nonNullString">
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ
					This represents the physiologically significant time of the observation which for lab tests equals the
					specimen collection date/time and is therefore redundant.	-->
			<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
			<!-- ARE THERE REALLY USE CASES FOR THESE: Method and Date/Time of Analysis? -->
			<!-- Method: Identifies specific NAHLN method version or for future use, local method -->
			<xsd:element name="OBX.17" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- OID for method -->
						<xsd:element name="CWE.1" type="OID" minOccurs="0"/>
						<!-- Text for method -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO" minOccurs="0"/>
						<!-- Alternate code for method -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alternate text for method -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- "L" for local code in most cases -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Date/Time of analysis (when test was set up) YYYYMMDDHHMMSS-ZZZZ -->
			<xsd:element name="OBX.19" minOccurs="0" type="CondDateTime"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="OPL_O37.SPECIMEN">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Specimen Segment -->
				<xsd:element ref="SPM"/>
				<!-- Zero or more observations on the specimen -->
				<xsd:element name="OBX" type="SPECIMEN.OBX" minOccurs="0" maxOccurs="unbounded"/>
				<!-- Zero or more containers (outer packaging) -->
				<xsd:element ref="OPL_O37.CONTAINER" minOccurs="0" maxOccurs="unbounded"/>
				<!-- One or more test requests for the specimen -->
				<xsd:element ref="OPL_O37.OBSERVATION_REQUEST" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SPM">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Specimen Identifier -->
				<xsd:element name="SPM.2">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Placer specimen ID -->
							<xsd:element name="EIP.1" minOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Placer Assigned specimen id for specimen or pool -->
										<xsd:element name="EI.1" minOccurs="1" maxOccurs="1">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
													<xsd:maxLength value="32"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Text for placer ID system -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Or Lab Constant either Lab OID or Lab OID for placer specimen IDs -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- If above is an OID, "ISO" otherwise omit -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Filler (Lab) assigned ID 
							     Used for referral tests to carry original lab's ID (debatable usage IMHO) -->
							<xsd:element name="EIP.2" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab Assigned specimen id for specimen or pool -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Text namespace ID -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Lab Constant either Lab OID or Lab OID for specimen IDs -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Repeats for each individual specimen in the pool or single instance for sub-sample, alloquot, etc.
				     Not in Jim's Implementation Guide.  Do we need this? 
				     Used for referal specimens -->
				<xsd:element name="SPM.3" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Placer assigned parent specimen ID -->
							<xsd:element name="EIP.1" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Placer Assigned specimen id for specimen -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Text for placer ID system -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Or Lab Constant either Lab OID or Lab OID for placer specimen IDs -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- If above is an OID, "ISO" otherwise omit -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Type -->
				<xsd:element name="SPM.4">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen type-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alternate code for specimen type-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen type -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Type Modifier (Not in result.  Do we need this? -->
				<xsd:element name="SPM.5" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen type-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alternate code for specimen type-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen type -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Additives -->
				<xsd:element name="SPM.6" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen additive-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen additive -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70371" -->
							<xsd:element name="CWE.3" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70371"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Alternate code for specimen additive-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen additive -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Source Site -->
				<xsd:element name="SPM.8" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen source site-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen source site -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alternate code for specimen source site-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen source site -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Source Site Modifier (Not in result.  Do we need this? -->
				<xsd:element name="SPM.9" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen source site-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen source site -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alternate code for specimen source site-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen source site -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Role: Patient sample, pool, grouped specimen, etc. -->
				<xsd:element name="SPM.11">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN "P" Patient, "L" Pool, "G" Group -->
							<xsd:element name="CWE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Patient -->
										<xsd:enumeration value="P"/>
										<!-- Pool -->
										<xsd:enumeration value="L"/>
										<!-- Group -->
										<xsd:enumeration value="G"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN "Patient", "Pooled", or "Group" -->
							<xsd:element name="CWE.2" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- If CWE.1 = P -->
										<xsd:enumeration value="Patient"/>
										<!-- If CWE.1 = L -->
										<xsd:enumeration value="Pooled"/>
										<!-- If CWE.1 = G -->
										<xsd:enumeration value="Group"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN Constant "HL70369" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70369"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Grouped Specimen Count -->
				<xsd:element name="SPM.13" type="xsd:int" minOccurs="0" maxOccurs="1"/>
				<!-- Specimen Description -->
				<xsd:element name="SPM.14" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<!-- Special Handling Code -->
				<xsd:element name="SPM.15" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for Special Handling Code-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for Special Handling Code -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70376" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70376"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Alternate code for Special Handling Code-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for Special Handling Code -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Risk Code -->
				<xsd:element name="SPM.16" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for Specimen Risk Code-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for Specimen Risk Code -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70489" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70489"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Alternate code for Specimen Risk Code-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for Specimen Risk Code -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Collection Date/Time: If available is the most important date/time for Epi -->
				<xsd:element name="SPM.17" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Start time used since collection is virtually instantaneous for our samples. YYYYMMDDHHMM-ZZZZ -->
							<!-- TODO Fix this in result message was NonNullString.  Why?  Do we need DR.2 in Result for non-NAHLN use?-->
							<xsd:element name="DR.1" type="DateTime"/>
							<!-- End time used very rarely for collections that take place over a distict period (24 hour urine, etc.) -->
							<xsd:element name="DR.2" type="DateTime" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen condition: Condition when shipped in order message (when received in result) -->
				<xsd:element name="SPM.24" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code for specimen condition ( FROZ, COOL, etc, see Implementation Guide for Codes)-->
							<xsd:element name="CWE.1" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<!-- Autolyzed -->
										<xsd:enumeration value="AUT"/>
										<!-- Clotted -->
										<xsd:enumeration value="CLOT"/>
										<!-- Contaminated -->
										<xsd:enumeration value="CON"/>
										<!-- Cool -->
										<xsd:enumeration value="COOL"/>
										<!-- Frozen -->
										<xsd:enumeration value="FROZ"/>
										<!-- Hemolyzed -->
										<xsd:enumeration value="HEM"/>
										<!-- Live -->
										<xsd:enumeration value="LIVE"/>
										<!-- Room Temperature -->
										<xsd:enumeration value="ROOM"/>
										<!-- Sample Not Received -->
										<xsd:enumeration value="SNR"/>
										<!-- Warm -->
										<xsd:enumeration value="WRM"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Text for specimen condition -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70493" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70493"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Alternate code for specimen condition-->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for specimen condition -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Number of Specimen Containers -->
				<xsd:element name="SPM.26" type="xsd:int" minOccurs="0"/>
				<!-- Container Type -->
				<xsd:element name="SPM.27" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for container type once available -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for container type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Alternate code for container type (If local system codes container types, include in 4-6) -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for container type -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text (only component used for now) -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END SPM -->

	<!-- NOTE:  This is a much more complete OBX than the result message
		        which was ONLY to support number of swabs per pool.  -->
	<xsd:complexType name="SPECIMEN.OBX">
		<xsd:sequence>
			<!-- OBX.2 is not present only if OBX.11 is valued X -->
			<xsd:element name="OBX.2" minOccurs="0">
				<!-- Result type code CWE, NM, SN, ST -->
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<!-- Coded With Exceptions -->
						<xsd:enumeration value="CWE"/>
						<!-- Numeric -->
						<xsd:enumeration value="NM"/>
						<!-- Structured Numeric -->
						<xsd:enumeration value="SN"/>
						<!-- String -->
						<xsd:enumeration value="ST"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OBX.3">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Loinc code of result -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Loinc short name -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "LN" -->
						<xsd:element name="CWE.3" type="LN"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Observation subID: For future use.  Will be used if multiple observations must be
						related to one another in specific ways.  -->
			<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
			<!-- Actual Test Result Value -->
			<xsd:element name="OBX.5" minOccurs="0">
				<!-- Our definition of ANY because this may support various types of observations -->
				<!-- mixed="true" allows for ST and NM types -->
				<xsd:complexType mixed="true">
					<xsd:sequence>
						<!-- Comparator: =, <, <=, >, >= -->
						<xsd:element name="SN.1" type="xsd:string" minOccurs="0"/>
						<!-- Number 1 -->
						<xsd:element name="SN.2" type="xsd:string" minOccurs="0"/>
						<!-- Separator: :, -, etc.-->
						<xsd:element name="SN.3" type="xsd:string" minOccurs="0"/>
						<!-- Number 2 -->
						<xsd:element name="SN.4" type="xsd:string" minOccurs="0"/>
						<!-- Code -->
						<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
						<!-- Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- Code System -->
						<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Units if applicable -->
			<xsd:element name="OBX.6" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Units Code -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Units Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Abnormal Flags: Interpretation (See Implementation Guide for Codes) -->
			<xsd:element name="OBX.8" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<!-- NAHLN Constant "F" Only final results messaged -->
			<xsd:element name="OBX.11">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="F"/>
						<!-- Final Result -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ
						This represents the physiologically significant time of the observation which for lab tests equals the
						specimen collection date/time and is therefore redundant.	-->
			<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

	<!-- Container holding specimen.  Order will arrive before the shipment in most
    cases.  This informs the lab what "box" to be looking for and for tracking. -->
	<xsd:element name="OPL_O37.CONTAINER">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="SAC" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Only segment in the CONTAINER Group -->
	<xsd:element name="SAC">
		<!-- Note: GROUP is optional and repeatable -->
		<xsd:complexType>
			<xsd:sequence>
				<!-- External Accession ID -->
				<xsd:element name="SAC.1" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- External Accession ID for referral tests -->
							<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
							<!-- Either text of placer order number system -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
							<!-- Or OID for placer order number system -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- If OID, this is "ISO" otherwise omitted -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Container Identifier -->
				<xsd:element name="SAC.3" minOccurs="0" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Identifier on individual specimen container -->
							<xsd:element name="EI.1" type="nonNullString"/>
							<!-- Either text of number system -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
							<!-- Or OID for number system -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- If OID, this is "ISO" otherwise omitted -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Carrier Type: Used in NAHLN to identify the type of outer
					package in which the specimen container(s) will be delivered
					There is no standard coding system so often only Text or Original Text
					will be sent. -->
				<xsd:element name="SAC.9">
					<xsd:complexType>
						<xsd:sequence>
							<!-- If a code is used for carrier type put here -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Description of code or just description -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- If a code is used, the system goes here -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Original text  -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Carrier Identifier -->
				<xsd:element name="SAC.10">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Identifier on outer carrier -->
							<xsd:element name="EI.1" type="nonNullString"/>
							<!-- Either text of number system -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
							<!-- Or OID for number system -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- If OID, this is "ISO" otherwise omitted -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Each order on the same specimen forms a repeat of this group. -->
	<xsd:element name="OPL_O37.OBSERVATION_REQUEST">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Information common to list of orders except here one to one relationship -->
				<xsd:element ref="ORC" minOccurs="0"/>
				<!-- General order information -->
				<xsd:element ref="OBR"/>
				<!-- Priority -->
				<xsd:element name="OPL_O37.TIMING" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="TQ1" minOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="OBX" type="ORDER.OBX" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ORC">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NAHLN constant (for new orders at least)  "NW"
					This field is more complicated than it looks.  The first electronic message with this order
					should be flagged NW for new.  Subsequent messages will be SC for status change.
					When sending results for which there is no corresponding electronic order message,
					this field is NW.  If responding to an electronic order (still call "unsolicited in HL7-speak)
					this will be SC.							-->
				<xsd:element name="ORC.1">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="SC"/>
							<xsd:enumeration value="NW"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- Placer Group Number: Used for referral numbers, etc. -->
				<xsd:element name="ORC.4" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Referral Number -->
							<xsd:element name="EI.1" type="nonNullString"/>
							<!-- Text of referral number system if not standard with OID -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0" maxOccurs="1"/>
							<!-- Referral Number Assigner OID
								As a workable default for now assume all referral numbers
								have been issued by the NAHLN so the OID is
								2.16.840.1.113883.3.5.1.8  -->
							<xsd:element name="EI.3" type="OID" minOccurs="0" maxOccurs="1"/>
							<!-- NAHLN Constant "ISO" -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0" maxOccurs="1"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Date/Time of transaction YYYYMMDDHHMMSS
					For the order this is the date/time that the order was placed.  -->
				<xsd:element name="ORC.9" type="DateTime" minOccurs="0"/>
				<!-- Entering Device: Identifier for PC, terminal, etc., used to enter order (why?) 
					 You don't normally need to identify entering device, especially if 
					 included in SFT.  But if you need to know which handset was used, etc.,
					 it can go here.-->
				<xsd:element name="ORC.18" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code for device -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- String ID of device -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Code system if code is provided Often "L"-->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Alternate code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text  -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="OBR">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Placer Order Number -->
				<xsd:element name="OBR.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Order number as received from field -->
							<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
							<!-- Either text of placer order number system -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
							<!-- Or OID for placer order number system -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- If OID, this is "ISO" otherwise omitted -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Order Service Identifier.  What you are ordering -->
				<xsd:element name="OBR.4">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Loinc code for order if one exists -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Loinc short name -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "LN" -->
							<xsd:element name="CWE.3" type="LNorLocal" minOccurs="0"/>
							<!-- Alternate code for order such as local or NAHLN-defined battery code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alternate text for order -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- "L" for local code in most cases -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text  -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Parent Order Number: If this order is a reflex order from an identified
					parent order this is used to link them. -->
				<xsd:element name="OBR.29" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Filler Order Number -->
							<xsd:element name="EIP.2">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab assigned parent order number -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Lab Constant Lab OID or Lab's OID for Order Numbers -->
										<xsd:element name="EI.3" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Reason for Study: Corresponds to OBX.5 in Result message observation on visit
					This cardinality is better in that version but the order message has no visit! -->
				<xsd:element name="OBR.31" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- In normal usage CWE.1 - CWE.3 will be used and the rest not used
								the optionality is retained for future use and for unusual cases where
								the reson for visit is not anticipated by NAHLN codes.
								This could need to be redefined as one of our ANY fields depending on future
								uses of observation on the visit -->
							<!-- Result NAHLN code for reason for visit: S Surveillance, D Diagnostic, etc. -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Result Text: "Surveillance, etc." -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Constant "L" -->
							<xsd:element name="CWE.3" type="Local" minOccurs="0"/>
							<!-- Local code for reason for visit: S Surveillance, D Diagnostic, etc. -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Result Text: "Surveillance, etc." -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text of reason for visit -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="TQ1">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="TQ1.9" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for priority  -->
							<!-- TODO: Publish standard list of SNOMED priority codes -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Text: "Normal Priority" etc. -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" until code system for this gets established -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Local, NAHLN or FADDL code for priority -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: "Surveillance, etc." -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" unless other coding system gets developed-->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text of priority -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="ORDER.OBX">
		<xsd:sequence>
			<!-- Currently NAHLN constant "NM" but schema supports other value types FUO -->
			<xsd:element name="OBX.2">
				<!-- Result type code CWE, NM, SN, ST -->
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<!-- Coded With Exceptions -->
						<xsd:enumeration value="CWE"/>
						<!-- Numeric -->
						<xsd:enumeration value="NM"/>
						<!-- Structured Numeric -->
						<xsd:enumeration value="SN"/>
						<!-- String -->
						<xsd:enumeration value="ST"/>
						<!-- Date -->
						<xsd:enumeration value="DTM"/>

					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Observation Identifier -->
			<xsd:element name="OBX.3">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Loinc code of result -->
						<xsd:element name="CWE.1" type="nonNullString" minOccurs="0"/>
						<!-- Loinc short name -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "LN" -->
						<xsd:element name="CWE.3" type="LN" minOccurs="0"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Observation sub id -->
			<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
			<!-- Observation Value -->
			<xsd:element name="OBX.5">
				<!-- Our definition of ANY because this may support various types of observations about the patient -->
				<!-- mixed="true" allows for ST and NM types -->
				<xsd:complexType mixed="true">
					<xsd:sequence>
						<!-- Comparator: =, <, <=, >, >= -->
						<xsd:element name="SN.1" type="xsd:string" minOccurs="0"/>
						<!-- Number 1 -->
						<xsd:element name="SN.2" type="xsd:string" minOccurs="0"/>
						<!-- Separator: :, -, etc.-->
						<xsd:element name="SN.3" type="xsd:string" minOccurs="0"/>
						<!-- Number 2 -->
						<xsd:element name="SN.4" type="xsd:string" minOccurs="0"/>
						<!-- Code -->
						<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
						<!-- Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- Code System -->
						<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Units if applicable -->
			<xsd:element name="OBX.6" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Units Code -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Units Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Interpretation of result "Abornomal flags" prior to version 2.7 -->
			<xsd:element name="OBX.8" type="xsd:string" minOccurs="0"/>
			<!-- NAHLN Constant "F" Only final results messaged -->
			<xsd:element name="OBX.11" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="nonNullString">
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ
					This represents the physiologically significant time of the observation which for lab tests equals the
					specimen collection date/time and is therefore redundant.	-->
			<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
			<!-- ARE THERE REALLY USE CASES FOR THESE: Method and Date/Time of Analysis? -->
			<!-- Method: Identifies specific NAHLN method version or for future use, local method -->
			<xsd:element name="OBX.17" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- OID for method -->
						<xsd:element name="CWE.1" type="OID" minOccurs="0"/>
						<!-- Text for method -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO" minOccurs="0"/>
						<!-- Alternate code for method -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alternate text for method -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- "L" for local code in most cases -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Date/Time of analysis (when test was set up) YYYYMMDDHHMMSS-ZZZZ -->
			<xsd:element name="OBX.19" minOccurs="0" type="CondDateTime"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- END OBSERVATION REQUEST Group -->

	<!-- PRIOR RESULT Group -->
	<!-- This group is used primarily for confirmatory testing.  This group contains the 
		results that are to be followed up with additional tests at the receiving lab.  It
		is expected at this group be populated with clones of the result message segments:
		NK1, PV1, OBR, and OBX for the positive screening tests to be confirmed. -->
	<xsd:element name="OPL_O37.PRIOR_RESULT">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NK1 Present mainly as a placeholder.  May be empty except for sequence # 
				     The rest of the structure follows the NK1 above -->
				<xsd:element ref="NK1" minOccurs="1" maxOccurs="1"/>
				<!-- PATIENT_VISIT_PRIOR allows communication of original accession
					 information such as accession # and case coordinator -->
				<xsd:element ref="OPL_O37.PATIENT_VISIT_PRIOR" minOccurs="0" maxOccurs="1"/>
				<!-- ORDER_PRIOR repeats once for each result that is needed for the 
					 tests ordered in this ORDER group.  (What is being confirmed?) -->
				<xsd:element ref="OPL_O37.ORDER_PRIOR" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- See NK1 definition for OPL_O37.ORDER.  Structures are identical -->

	<!-- Patient Visit Prior is optional but is the source of the original 
					lab's accession number so highly encouraged. 
				     This group consists of only one segment, PV1. -->
	<xsd:element name="OPL_O37.PATIENT_VISIT_PRIOR">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="PV1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant "C" Not used here but required for HL7 compliance-->
							<xsd:element name="PV1.2">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="C"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Attending doctor is Responsible Diagnostician for NAHLN messages -->
							<xsd:element name="PV1.7" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab's identifier for the diagnostician -->
										<xsd:element name="XCN.1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="XCN.2">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Last name of diagnostician -->
													<xsd:element name="FN.1" type="nonNullString"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!-- First Name of diagnostician -->
										<xsd:element name="XCN.3" type="xsd:string" minOccurs="0"/>
										<!-- Middle Name of diagnostician -->
										<xsd:element name="XCN.4" type="xsd:string" minOccurs="0"/>
										<!-- Name suffix (sr. jr. etc.) of diagnostician -->
										<xsd:element name="XCN.5" type="xsd:string" minOccurs="0"/>
										<!--Assigning authority of ID in XCN.1 -->
										<xsd:element name="XCN.9" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Lab's OID or OID of Lab's Diagnostician number system -->
													<xsd:element name="HD.2" type="OID"/>
													<!-- NAHLN Constant "ISO" -->
													<xsd:element name="HD.3" type="ISO"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Accession number -->
							<xsd:element name="PV1.19">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Insert Accession Number -->
										<xsd:element name="CX.1">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="30"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="CX.4">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Lab constant Lab OID or Lab's OID for Accession number -->
													<xsd:element name="HD.2" type="OID"/>
													<!-- NAHLN Constant "ISO" -->
													<xsd:element name="HD.3" type="ISO"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- NAHLN Constant "V" not used here but required for HL7 compliance-->
							<xsd:element name="PV1.51" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Include as many order/result pairs as needed to justify new orders 
                	included in this message.  -->
	<xsd:element name="OPL_O37.ORDER_PRIOR">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="OBR" minOccurs="1" maxOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Placer Order Number -->
							<xsd:element name="OBR.2" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Order number as received from field -->
										<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
										<!-- Either text of placer order number system -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Or OID for placer order number system -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- If OID, this is "ISO" otherwise omitted -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Filler (Lab) Order Number -->
							<xsd:element name="OBR.3">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab assigned order number -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Lab Constant Lab OID or Lab's OID for Order Numbers -->
										<xsd:element name="EI.3" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Order Service Identifier -->
							<xsd:element name="OBR.4">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Loinc code for order if one exists -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Loinc short name -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "LN" -->
										<xsd:element name="CWE.3" type="LN" minOccurs="0"/>
										<!-- Alternate code for order such as local or NAHLN-defined battery code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alternate text for order -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- "L" for local code in most cases -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original text  -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Date/Time report released YYYYMMDDHHMMSS-ZZZZ -->
							<xsd:element name="OBR.22" type="DateTime"/>
							<!-- Parent Order Number: If this order is a reflex order from an identified parent order this is used to link them. -->
							<xsd:element name="OBR.29" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Filler Order Number -->
										<xsd:element name="EIP.2">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Lab assigned parent order number -->
													<xsd:element name="EI.1" type="nonNullString"/>
													<!-- Lab Constant Lab OID or Lab's OID for Order Numbers -->
													<xsd:element name="EI.3" type="OID"/>
													<!-- NAHLN Constant "ISO" -->
													<xsd:element name="EI.4" type="ISO"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Reason for study -->
							<xsd:element name="OBR.31" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- NAHLN code for Reason N, S, etc. -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Description of code, National Surveillance, etc. -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "L" -->
										<xsd:element name="CWE.3" type="LocalCode" minOccurs="0"/>
										<!-- Alternate code for Reason -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alternate text for Reason -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- "L" for local code in most cases -->
										<!-- Note: Should only send primary OR local code in this case.
														It is technically an error to send two codes from the same
														coding system (L in this case). -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original text  -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- OBX repeats for multiple observations on one test order.
								This is the less common usage in NAHLN than one OBR per OBX. -->
				<xsd:element name="OBX" minOccurs="1" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- OBX.2 is not present only if OBX.11 is valued X -->
							<xsd:element name="OBX.2" minOccurs="0">
								<!-- Result type code CWE, NM, SN, ST -->
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="CWE"/>
										<!-- Coded With Exceptions -->
										<xsd:enumeration value="NM"/>
										<!-- Numeric -->
										<xsd:enumeration value="SN"/>
										<!-- Structured Numeric -->
										<xsd:enumeration value="ST"/>
										<!-- String -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="OBX.3">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="CWE.1" type="nonNullString"/>
										<!-- Loinc code of result -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Loinc short name -->
										<xsd:element name="CWE.3" type="LN"/>
										<!-- NAHLN Constant "LN" -->
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Observation subID: For future use.  Will be used if multiple observations must be
								related to one another in specific ways.  -->
							<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
							<!-- Actual Test Result Value -->
							<xsd:element name="OBX.5" minOccurs="0">
								<!-- Our definition of ANY because this may support various types of observations -->
								<!-- mixed="true" allows for ST and NM types -->
								<xsd:complexType mixed="true">
									<xsd:sequence>
										<!-- Comparator: =, <, <=, >, >= -->
										<xsd:element name="SN.1" type="xsd:string" minOccurs="0"/>
										<!-- Number 1 -->
										<xsd:element name="SN.2" type="xsd:string" minOccurs="0"/>
										<!-- Separator: :, -, etc.-->
										<xsd:element name="SN.3" type="xsd:string" minOccurs="0"/>
										<!-- Number 2 -->
										<xsd:element name="SN.4" type="xsd:string" minOccurs="0"/>
										<!-- Code -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Code System -->
										<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alt Text -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code System -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Units if applicable -->
							<xsd:element name="OBX.6" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Units Code -->
										<xsd:element name="CWE.1" type="nonNullString"/>
										<!-- Units Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN constant "ISO" -->
										<xsd:element name="CWE.3" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Abnormal Flags: Interpretation (See Implementation Guide for Codes) -->
							<xsd:element name="OBX.8" type="xsd:string" minOccurs="0" maxOccurs="1"/>
							<!-- NAHLN Constant "F" Only final results messaged -->
							<xsd:element name="OBX.11">
								<!-- Slinke 11/27/07, Added enum values to support Spec Rct Rsns -->
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="F"/>
										<!-- Final Result -->
										<xsd:enumeration value="X"/>
										<!-- Specimen Rejected, No Result -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ 
								This represents the physiologically significant time of the observation which for lab tests equals the 
								specimen collection date/time and is therefore redundant.	-->
							<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
							<!-- Responsible Observer(s) Will represent the proficiency tested technician who performed the test-->
							<xsd:element name="OBX.16" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab's identifier for the observer -->
										<xsd:element name="XCN.1" type="xsd:string" minOccurs="0"/>
										<xsd:element name="XCN.2">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Last name of observer  -->
													<xsd:element name="FN.1" type="nonNullString"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!-- First Name of observer  -->
										<xsd:element name="XCN.3" type="xsd:string" minOccurs="0"/>
										<!-- Middle Name of observer  -->
										<xsd:element name="XCN.4" type="xsd:string" minOccurs="0"/>
										<!-- Name suffix (sr. jr. etc.) of observer  -->
										<xsd:element name="XCN.5" type="xsd:string" minOccurs="0"/>
										<!--Assigning authority of ID in XCN.1 -->
										<xsd:element name="XCN.9" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
													<!-- Lab's OID or OID of Lab's employee number system -->
													<xsd:element name="HD.2" type="OID"/>
													<!-- NAHLN Constant "ISO" -->
													<xsd:element name="HD.3" type="ISO"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Method: Identifies specific NAHLN method version or for future use, local method -->
							<xsd:element name="OBX.17" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- OID for method -->
										<xsd:element name="CWE.1" type="OID" minOccurs="0"/>
										<!-- Text for method -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="CWE.3" type="ISO" minOccurs="0"/>
										<!-- Alternate code for method -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alternate text for method -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- "L" for local code in most cases -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Equipment Instance Identifier: IDs specific machine(s) used. -->
							<xsd:element name="OBX.18" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Equipment Instance ID -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Equipment Instance ID OID-->
										<xsd:element name="EI.3" type="OID"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Date/Time of analysis (when test was set up) YYYYMMDDHHMMSS-ZZZZ -->
							<xsd:element name="OBX.19" minOccurs="0" type="CondDateTime"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>

			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
