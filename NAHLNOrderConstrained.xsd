<?xml version="1.0" encoding="UTF-8"?>
<!-- ******* Version 0.0 of the NAHLN Base Order Schema ******* -->
<!--
This is the basic schema for all NAHLN laboratory order messages.  

It includes all fields that we anticipate needing in any of the disease 
program or surveillance testing.  It requires only the minimum needed 
for all types of testing.	

This version does not put EpiData in an external namespace.  
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:hl7="urn:hl7-org:v2xml">
	<xsd:simpleType name="OID">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\s)*(([1-9][0-9]*)|(0))(\.(([1-9][0-9]*)|(0)))+(\s)*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="COUNTRYCODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Area Code -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-9]{1}\d*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AREACODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Area Code -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[2-9]{1}\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PHONENUMBER">
		<!-- 11/30/2007 Slinke Added Expression to validate Phone Num -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EXTENSION">
		<!-- 11/30/2007 Slinke Added Expression to validate Extension -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{1,5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ISO">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="ISO"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UNITS">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ISO+"/>
			<xsd:enumeration value="ANS+"/>
			<xsd:enumeration value="UCUM"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LN">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LN"/>
			<!-- if sending real LOINC -->
			<!-- where do we use this type intending to allow either? -->
			<xsd:enumeration value="L"/>
			<!-- if you must send local codes -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- HL7 coding system constant for local code -->
	<xsd:simpleType name="Local">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="L"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LNorLocal">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LN"/>
			<xsd:enumeration value="L"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern
				value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CondDateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern
				value="(\d{4}(((0[1-9])|(1[0-2]))(((0[1-9])|([1-2]\d)|(3[0-1]))((([01]\d|2[0-3])([0-5]\d))(([0-5]\d)((\.\d{1,4}))?)?)?)?)?([+\-](([0]\d|1[0-3])([0-5]\d)))?)?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StateCode">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AK"/>
			<xsd:enumeration value="AL"/>
			<xsd:enumeration value="AR"/>
			<xsd:enumeration value="AS"/>
			<xsd:enumeration value="AZ"/>
			<xsd:enumeration value="CA"/>
			<xsd:enumeration value="CO"/>
			<xsd:enumeration value="CT"/>
			<xsd:enumeration value="DC"/>
			<xsd:enumeration value="DE"/>
			<xsd:enumeration value="FL"/>
			<xsd:enumeration value="FM"/>
			<xsd:enumeration value="GA"/>
			<xsd:enumeration value="GB"/>
			<xsd:enumeration value="GU"/>
			<xsd:enumeration value="HI"/>
			<xsd:enumeration value="IA"/>
			<xsd:enumeration value="ID"/>
			<xsd:enumeration value="IL"/>
			<xsd:enumeration value="IN"/>
			<xsd:enumeration value="KS"/>
			<xsd:enumeration value="KY"/>
			<xsd:enumeration value="LA"/>
			<xsd:enumeration value="MA"/>
			<xsd:enumeration value="MD"/>
			<xsd:enumeration value="ME"/>
			<xsd:enumeration value="MH"/>
			<xsd:enumeration value="MI"/>
			<xsd:enumeration value="MN"/>
			<xsd:enumeration value="MO"/>
			<xsd:enumeration value="MP"/>
			<xsd:enumeration value="MS"/>
			<xsd:enumeration value="MT"/>
			<xsd:enumeration value="NC"/>
			<xsd:enumeration value="ND"/>
			<xsd:enumeration value="NE"/>
			<xsd:enumeration value="NH"/>
			<xsd:enumeration value="NJ"/>
			<xsd:enumeration value="NM"/>
			<xsd:enumeration value="NV"/>
			<xsd:enumeration value="NY"/>
			<xsd:enumeration value="OH"/>
			<xsd:enumeration value="OK"/>
			<xsd:enumeration value="OR"/>
			<xsd:enumeration value="PA"/>
			<xsd:enumeration value="PR"/>
			<xsd:enumeration value="PW"/>
			<xsd:enumeration value="PZ"/>
			<xsd:enumeration value="RI"/>
			<xsd:enumeration value="SC"/>
			<xsd:enumeration value="SD"/>
			<xsd:enumeration value="TN"/>
			<xsd:enumeration value="TX"/>
			<xsd:enumeration value="UM"/>
			<xsd:enumeration value="UT"/>
			<xsd:enumeration value="VA"/>
			<xsd:enumeration value="VI"/>
			<xsd:enumeration value="VT"/>
			<xsd:enumeration value="WA"/>
			<xsd:enumeration value="WI"/>
			<xsd:enumeration value="WK"/>
			<xsd:enumeration value="WV"/>
			<xsd:enumeration value="WY"/>
			<xsd:enumeration value="ZK"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ZIPCODE">
		<!-- 11/30/2007 Slinke Added Expression to validate Extension -->
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="\d{5}|\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="YN">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="N"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IDType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ANON"/>
			<!-- Anonymous identifier -->
			<xsd:enumeration value="B"/>
			<!-- Band -->
			<xsd:enumeration value="BL"/>
			<!-- Band, Leg  -->
			<xsd:enumeration value="BR"/>
			<!-- Birth registry number -->
			<xsd:enumeration value="BRD"/>
			<!-- Brand -->
			<xsd:enumeration value="BRF"/>
			<!-- Brand, Freeze  -->
			<xsd:enumeration value="BRH"/>
			<!-- Brand, Hot Iron  -->
			<xsd:enumeration value="BRN"/>
			<!-- Breed Registry Number -->
			<xsd:enumeration value="CSID"/>
			<!-- Collection Site Identification number -->
			<xsd:enumeration value="FI"/>
			<!-- Facility ID -->
			<xsd:enumeration value="GI"/>
			<!-- Guarantor internal identifier -->
			<xsd:enumeration value="GN"/>
			<!-- Guarantor external  identifier -->
			<xsd:enumeration value="GRID"/>
			<!-- Group Identifier -->
			<xsd:enumeration value="LR"/>
			<!-- Local Registry ID -->
			<xsd:enumeration value="MCN"/>
			<!-- Microchip Number -->
			<xsd:enumeration value="MR"/>
			<!-- Medical record number -->
			<xsd:enumeration value="MRT"/>
			<!-- Temporary Medical Record Number -->
			<xsd:enumeration value="N"/>
			<!-- Notch -->
			<xsd:enumeration value="NCHN"/>
			<!-- Neck Chain Identifier -->
			<xsd:enumeration value="NE"/>
			<!-- Notch, Ear  -->
			<xsd:enumeration value="NF"/>
			<!-- Notch, Fin  -->
			<xsd:enumeration value="PE"/>
			<!-- Living Subject Enterprise Number -->
			<xsd:enumeration value="PNT"/>
			<!-- Temporary Living Subject Number -->
			<xsd:enumeration value="PT"/>
			<!-- Patient external identifier -->
			<xsd:enumeration value="QA"/>
			<!-- QA number -->
			<xsd:enumeration value="RRI"/>
			<!-- Regional Registry Number -->
			<xsd:enumeration value="SR"/>
			<!-- State Registry Number -->
			<xsd:enumeration value="TAG"/>
			<!-- Tag -->
			<xsd:enumeration value="TAGB"/>
			<!-- Tag, Back -->
			<xsd:enumeration value="TAGC"/>
			<!-- Tag, FSIS Condemnation  -->
			<xsd:enumeration value="TAGE"/>
			<!-- Tag, Ear -->
			<xsd:enumeration value="TAGF"/>
			<!-- Tag, Fin  -->
			<xsd:enumeration value="TAGN"/>
			<!-- Tag, Neck  -->
			<xsd:enumeration value="TAGOB "/>
			<!-- Tag, Official USDA Bangle  -->
			<xsd:enumeration value="TAGOE"/>
			<!-- Tag, Owner Ear -->
			<xsd:enumeration value="TAGOS"/>
			<!-- Tag, Official USDA Silver  -->
			<xsd:enumeration value="TAGOV"/>
			<!-- Tag, Official USDA Vaccination  -->
			<xsd:enumeration value="TAGT"/>
			<!-- Tag, Tail -->
			<xsd:enumeration value="TAGW"/>
			<!-- Tag, Wing  -->
			<xsd:enumeration value="TAT"/>
			<!-- Tattoo -->
			<xsd:enumeration value="TATB"/>
			<!-- Tattoo, Body  -->
			<xsd:enumeration value="TATE"/>
			<!-- Tattoo, Ear  -->
			<xsd:enumeration value="TATL"/>
			<!-- Tattoo, Lip  -->
			<xsd:enumeration value="U"/>
			<!-- Unspecified identifier -->
			<xsd:enumeration value="USAIN"/>
			<!-- United Stated Animal Identification Number -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SCT">
		<!-- Snomed Coding System -->
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SCT"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="nonNullString">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="OPL_O37">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Header consists of MSH for Routing and PV1 for Accession Number -->
				<xsd:element ref="MSH"/>
				<xsd:element ref="SFT" minOccurs="0" maxOccurs="unbounded"/>
				<!-- Submitter needed by lab.  Not always sent back in result message so required here -->
				<xsd:element ref="ROL" minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element ref="GT1" minOccurs="1" maxOccurs="unbounded"/>
				<!-- Followed by one or more animal or pool groups (Orders)
				     NOTE: Naming is confusing relative to the Result message -->
				<xsd:element ref="OPL_O37.ORDER" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- The MSH segment is almost all either NAHLN or Lab Constants 
		Insert date/time of message and message control id at run time.
	 -->
	<xsd:element name="MSH">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NAHLN Constant | -->
				<xsd:element name="MSH.1" minOccurs="1">
					<xsd:simpleType>
						<xsd:restriction base="nonNullString">
							<xsd:enumeration value="|"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<!-- NAHLN Constant ^~\&amp; -->
				<xsd:element name="MSH.2" minOccurs="1">
					<xsd:simpleType>
						<!--4/30/2008 Slinke.  Removed enum value due to rhapsody bug-->
						<xsd:restriction base="nonNullString"/>
					</xsd:simpleType>
				</xsd:element>
				<!-- Sending application -->
				<xsd:element name="MSH.3" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Lab Constant Application Abbrev -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- Lab Constant Application Names OID -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Sending facility -->
				<xsd:element name="MSH.4">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Sender Constant PremID or Non-producer participant ID -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- Sender Constant 2.16.840.1.113883.3.5.6.1.[1 or 4] (PIN or NPPN) -->
							<!-- 04/29/2008 Slinke. Changed from type OID to enumerated list. -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Receiving application -->
				<xsd:element name="MSH.5" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Application Abbrev -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- Lab OID or Lab NAHLN Application OID -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Receiving facility -->
				<xsd:element name="MSH.6" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- PIN or NPN of Lab -->
							<xsd:element name="HD.1" type="nonNullString"/>
							<!-- 2.16.840.1.113883.3.5.6.1.[1 or 4] (PIN or NPPN) -->
							<xsd:element name="HD.2" type="OID"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="HD.3" type="ISO"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Insert date/time of message as YYYYMMDDHHMMSS-ZZZZ -->
				<xsd:element name="MSH.7" type="DateTime" minOccurs="1"/>
				<!-- HL7 Message type -->
				<xsd:element name="MSH.9" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant OPL -->
							<xsd:element name="MSG.1" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="OPL"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN Constant O37 -->
							<xsd:element name="MSG.2" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="O37"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN Constant OPL_O37  -->
							<xsd:element name="MSG.3" minOccurs="1">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="OPL_O37"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Insert lab identifier for message used to match up errors, etc.  -->
				<xsd:element name="MSH.10" type="nonNullString"/>
				<!-- Processing ID -->
				<xsd:element name="MSH.11">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant P, Production or D, Debugging or T, Training -->
							<xsd:element name="PT.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="P"/>
										<!-- Production -->
										<xsd:enumeration value="D"/>
										<!-- Debugging -->
										<xsd:enumeration value="T"/>
										<!-- Training -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Version ID -->
				<xsd:element name="MSH.12">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Constant 2.6 -->
							<xsd:element name="VID.1" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Message profile -->
				<xsd:element name="MSH.21" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN Profile ID -->
							<xsd:element name="EI.1" minOccurs="1" type="nonNullString"/>
							<!-- NAHLN Constant OID for NAHLN Profile IDs 2.16.840.1.113883.3.5.9 -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- NAHLN Constant ISO -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END MSH -->
	<!-- Software: Defines each system handling the message one repeat per system.  If 
         only used for transmission, not required, e.g. cURL -->
	<xsd:element name="SFT">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Software Vendor -->
				<xsd:element name="SFT.1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Vendor Name -->
							<xsd:element name="XON.1" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Version or Release -->
				<xsd:element name="SFT.2" type="xsd:string" minOccurs="0"/>
				<!-- Product Name -->
				<xsd:element name="SFT.3" type="xsd:string"/>
				<!-- Binary ID (Hash, checksum, etc.) -->
				<xsd:element name="SFT.4" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Roles used for Submitter, Collector, Premises, etc. 
	     One instance for submitter required.  Others may be required by lab or program -->
	<xsd:element name="ROL">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Role instance ID.  Identifier qualifying this person to participate in this role. -->
				<xsd:element name="ROL.1" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Identifier, participant number, etc. -->
							<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
							<!-- Text namespace ID -->
							<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
							<!-- OID of identifier system such as collection program -->
							<xsd:element name="EI.3" type="OID" minOccurs="0"/>
							<!-- NAHLN Constant "ISO" -->
							<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Action code.  NAHLN Constant "UC" for unchanged.  Do not assign roles here, simply report existing ones. -->
				<xsd:element name="ROL.2" type="nonNullString"/>
				<!-- Role -->
				<xsd:element name="ROL.3">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN code for Role "SUB", "COL", "PREM", etc. -->
							<xsd:element name="CWE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="SUB"/>
										<!-- Submitter -->
										<xsd:enumeration value="COL"/>
										<!-- Collector -->
										<xsd:enumeration value="PREM"/>
										<!-- Source Premises -->
										<xsd:enumeration value="AT"/>
										<!-- Attending Veterinarian -->
										<xsd:enumeration value="PAR"/>
										<!-- Parent Company -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Spelled out role "Submitting Party", "Specimen Collector", "Source Premises", etc. -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70443" User table with all local codes -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="HL70443"/>
										<xsd:enumeration value="L"/> <!-- to allow current, incorrect, usage -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>	
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Role Person.  Who is the submitter, collector, etc.  Doesn't make sense for Premises but required placeholder -->
				<xsd:element name="ROL.4" minOccurs="1">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Person identifier -->
							<xsd:element name="XCN.1" type="xsd:string" minOccurs="0"/>
							<xsd:element name="XCN.2">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last Name ("Not Applicable" for Premises) -->
										<xsd:element name="FN.1">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
												<xsd:maxLength value="30"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name -->
							<xsd:element name="XCN.3" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name -->
							<xsd:element name="XCN.4" type="xsd:string" minOccurs="0"/>
							<!-- Name Suffix -->
							<xsd:element name="XCN.5" type="xsd:string" minOccurs="0"/>
							<!-- Assigning Authority -->
							<xsd:element name="XCN.9" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Text Namespace ID -->
										<xsd:element name="HD.1" type="xsd:string" minOccurs="0"/>
										<!-- OID of assigner of ID in XCN.1 -->
										<xsd:element name="HD.2" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="HD.3" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Address -->
				<xsd:element name="ROL.11" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XAD.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="nonNullString"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="ZIPCODE" minOccurs="0"/>
							<!-- Premises ID for PREM role only -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0" nillable="true"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
