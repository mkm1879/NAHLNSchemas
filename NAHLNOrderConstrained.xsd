	<!-- ORDER is roughly equivalent to the Accession Detail group in 
		the result message.  Repeats for each animal or pool. -->
	<xsd:element name="OPL_O37.ORDER">
		<xsd:complexType>
			<xsd:sequence>
				<!-- NK1 Represents the "Owner" who is the person most responsible for the animals or location being tested -->
				<!-- Really repeatable? One per relationship value. But then who is most responsible? -->
				<xsd:element ref="NK1" minOccurs="1" maxOccurs="unbounded"/>
				<xsd:element ref="OPL_O37.PATIENT" minOccurs="0"/>
				<!-- set to  in element definition below.-->
				<xsd:element ref="OPL_O37.SPECIMEN" maxOccurs="unbounded"/>
				<xsd:element ref="CTI" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END OPL_O37.ORDER -->

	<!-- Next of Kin: Owner -->
	<xsd:element name="NK1">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Sequence ID: NAHLN Constant "1" -->
				<xsd:element name="NK1.1" type="xsd:int"/>
				<!-- Owner Name -->
				<xsd:element name="NK1.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XPN.1" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Last name: "Not Provided" as default if not provided or known -->
										<xsd:element name="FN.1" type="nonNullString"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- First Name -->
							<xsd:element name="XPN.2" type="xsd:string" minOccurs="0"/>
							<!-- Middle Name -->
							<xsd:element name="XPN.3" type="xsd:string" minOccurs="0"/>
							<!-- Name Suffix -->
							<xsd:element name="XPN.4" type="xsd:string" minOccurs="0"/>
							<!-- Name type code -->
							<xsd:element name="XPN.7" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Relationship -->
				<xsd:element name="NK1.3" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN default "OWN" -->
							<xsd:element name="CNE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="ASC"/>
										<!-- Associate -->
										<xsd:enumeration value="EMC"/>
										<!-- Emergency Contact -->
										<xsd:enumeration value="GRD"/>
										<!-- Guardian -->
										<xsd:enumeration value="MGR"/>
										<!-- Manager -->
										<xsd:enumeration value="OWN"/>
										<!-- Owner -->
										<xsd:enumeration value="TRA"/>
										<!-- Trainer -->
										<xsd:enumeration value="UNK"/>
										<!-- Unknown -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN default "Owner" -->
							<xsd:element name="CNE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70063" -->
							<xsd:element name="CNE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70063"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>

						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner Address -->
				<xsd:element name="NK1.4" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="XAD.1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="SAD.1" type="nonNullString" minOccurs="1"/>
										<!-- Apartment number -->
										<xsd:element name="SAD.3" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Other designation Address Line 2 -->
							<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
							<!-- City -->
							<xsd:element name="XAD.3" type="xsd:string" minOccurs="0"/>
							<!-- State (Two character abbreviation) -->
							<xsd:element name="XAD.4" type="StateCode" minOccurs="0"/>
							<!-- Zipcode -->
							<xsd:element name="XAD.5" type="ZIPCODE" minOccurs="0"/>
							<!-- Geocoordinates -->
							<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
							<!-- County code -->
							<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner home phone(s) -->
				<xsd:element name="NK1.5" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: PRN -->
							<xsd:element name="XTN.2" type="nonNullString"/>
							<!-- Equipment type: PH Phone, -->
							<xsd:element name="XTN.3" type="nonNullString"/>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="xsd:int" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="xsd:int" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="xsd:int" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="xsd:int" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner business phone(s) -->
				<xsd:element name="NK1.6" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Use code: WPN -->
							<xsd:element name="XTN.2" type="nonNullString"/>
							<!-- Equipment type: PH Phone -->
							<xsd:element name="XTN.3" type="nonNullString"/>
							<!-- Email address -->
							<xsd:element name="XTN.4" type="xsd:string" minOccurs="0"/>
							<!-- Country code -->
							<xsd:element name="XTN.5" type="xsd:int" minOccurs="0"/>
							<!-- Area code -->
							<xsd:element name="XTN.6" type="xsd:int" minOccurs="0"/>
							<!-- Local number -->
							<xsd:element name="XTN.7" type="xsd:int" minOccurs="0"/>
							<!-- Extension -->
							<xsd:element name="XTN.8" type="xsd:int" minOccurs="0"/>
							<!-- Unformatted number if not parsable -->
							<xsd:element name="XTN.12" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner business name -->
				<xsd:element name="NK1.13" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Business Name -->
							<xsd:element name="XON.1" type="nonNullString"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Owner primary language -->
				<xsd:element name="NK1.20" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code See list of ISO abbreviations in implementation guide -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Text -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- Code System -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Alt Text -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- Alt Code System -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original Text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- PATIENT has one segment with only a few fields only species is critical here.  Production class is required by may be Unknown -->
	<xsd:element name="OPL_O37.PATIENT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="PID">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Patient Identifier(s) at least one required by HL7 -->
							<xsd:element name="PID.3" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Identifier (AIN or GIN preferred) -->
										<xsd:element name="CX.1" type="nonNullString"/>
										<!-- Assigning Authority -->
										<xsd:element name="CX.4" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
												<!-- Either Text of Assigner here -->
												<xsd:element name="HD.1" type="xsd:string" minOccurs="0"/>
												<!-- OR OID of Assigner (preferred) here -->
												<xsd:element name="HD.2" type="OID" minOccurs="0"/>
												<!-- If above is an OID, this is "ISO" if text, omit HD.3 -->
												<xsd:element name="HD.3" type="ISO" minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!-- Type code for type of ID such as eartag, microchip, etc. -->
										<xsd:element name="CX.5" type="IDType" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Patient Name: required for HL7 but often not sent for NAHLN -->
							<xsd:element name="PID.5">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="XPN.1" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
												<!-- Family Name "Not Provided" in most cases or animal name if given -->
												<xsd:element name="FN.1" type="nonNullString"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!-- Name type code -->
										<xsd:element name="XPN.7" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Date of birth to as much precision as known YYYYMMDD -->
							<xsd:element name="PID.7" type="CondDateTime" minOccurs="0"/>
							<!-- Administrative Sex (F,S,M,C,U, or X See Implementation Guide)-->
							<xsd:element name="PID.8" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="F"/>
										<!-- Female -->
										<xsd:enumeration value="S"/>
										<!-- Neutered Female -->
										<xsd:enumeration value="M"/>
										<!-- Male -->
										<xsd:enumeration value="C"/>
										<!-- Neutered Male -->
										<xsd:enumeration value="U"/>
										<!-- Gender Unknown -->
										<xsd:enumeration value="X"/>
										<!-- Multiple Genders -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Race: Used for color or markings in NAHLN -->
							<xsd:element name="PID.10" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Code -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Code System -->
										<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alt Text -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code System -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Patient Address -->
							<xsd:element name="PID.11" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Street Address -->
										<xsd:element name="XAD.1" minOccurs="0">
											<xsd:complexType>
												<xsd:sequence>
												<!-- Street Address -->
												<xsd:element name="SAD.1" type="nonNullString"/>
												<!-- Apartment number -->
												<xsd:element name="SAD.3" type="xsd:string"
												minOccurs="0"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!-- Other designation -->
										<xsd:element name="XAD.2" type="xsd:string" minOccurs="0"/>
										<!-- City -->
										<xsd:element name="XAD.3" type="xsd:string" minOccurs="0"/>
										<!-- State (Two character abbreviation) -->
										<xsd:element name="XAD.4" type="StateCode" minOccurs="0"/>
										<!-- Zipcode -->
										<xsd:element name="XAD.5" type="ZIPCODE" minOccurs="0"/>
										<!-- Premises ID  -->
										<xsd:element name="XAD.8" type="xsd:string" minOccurs="0"/>
										<!-- County:  Not in result.  Should be in my opinion -->
										<xsd:element name="XAD.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Birthplace (State) Not in Result Message. Should be?-->
							<xsd:element name="PID.23" type="StateCode" minOccurs="0"/>
							<!-- Date and time of death to as much precision as 
								known YYYYMMDDHHMM-ZZZZ -->
							<xsd:element name="PID.29" type="DateTime" minOccurs="0"/>
							<!-- Patient death indicator: "Y" if death, normally 
								omitted but my be "N" for live prior to testing -->
							<xsd:element name="PID.30" type="YN" minOccurs="0"/>
							<!-- Identity unknown indicator: "Y" for John Doe's, 
								normally omitted but may be set to "N" -->
							<xsd:element name="PID.31" type="YN" minOccurs="0"/>
							<!-- Taxonomic classification -->
							<xsd:element name="PID.35" minOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- SNOMED Code -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- SNOMED Preferred Name -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "SCT" -->
										<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
										<!-- Alt Code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alt Text -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code System -->
										<xsd:element name="CWE.6" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
												<xsd:enumeration value="L"/>
												<!-- Local Code -->
												<xsd:enumeration value="NAIS"/>
												<!-- NAIS Species Code -->
												<xsd:enumeration value="WS"/>
												<!-- Wildlife Services Species Code -->
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Strain -->
							<xsd:element name="PID.37" type="xsd:string" minOccurs="0"/>
							<!-- Production Class -->
							<xsd:element name="PID.38" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Production Type Code -->
										<xsd:element name="CWE.1" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
												<xsd:enumeration value="BR"/>
												<!-- Breeding/genetic shock -->
												<xsd:enumeration value="CP"/>
												<!-- Companion -->
												<xsd:enumeration value="DA"/>
												<!-- Dairy -->
												<xsd:enumeration value="DR"/>
												<!-- Draft -->
												<xsd:enumeration value="DU"/>
												<!-- Dual Purpose -->
												<xsd:enumeration value="EX"/>
												<!-- Exhibition -->
												<xsd:enumeration value="FB"/>
												<!-- Fiber -->
												<xsd:enumeration value="LY"/>
												<!-- Layer, including multiplier flocks -->
												<xsd:enumeration value="MP"/>
												<!-- Multiple Purpose -->
												<xsd:enumeration value="MT"/>
												<!-- Meat -->
												<xsd:enumeration value="NA"/>
												<!-- Not Applicable -->
												<xsd:enumeration value="NP"/>
												<!-- Non-Physical Product Production -->
												<xsd:enumeration value="OR"/>
												<!-- Ornamental -->
												<xsd:enumeration value="OT"/>
												<!-- Ither -->
												<xsd:enumeration value="PP"/>
												<!-- Physical Product Production -->
												<xsd:enumeration value="PL"/>
												<!-- Pleasure -->
												<xsd:enumeration value="RA"/>
												<!-- Racing -->
												<xsd:enumeration value="SH"/>
												<!-- Show -->
												<xsd:enumeration value="SV"/>
												<!-- Service -->
												<xsd:enumeration value="U"/>
												<!-- Unknown -->
												<xsd:enumeration value="WI"/>
												<!-- Wildlife -->
												<xsd:enumeration value="Z"/>
												<!-- Zoo -->
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant  "HL70429" -->
										<xsd:element name="CWE.3" minOccurs="0">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
												<xsd:enumeration value="HL70429"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Alt Code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Alt Text -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- Alt Code System -->
										<xsd:element name="CWE.6" type="Local" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Observations on the patient.  Note: In the result message this is a
					PATIENT_OBSERVATION group in the Order there is no group.  The OBX repeats
				    if necessary -->
				<xsd:element name="OBX" type="PATIENT.OBX" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END PATIENT -->

	<xsd:complexType name="PATIENT.OBX">
		<xsd:sequence>
			<!-- Currently NAHLN constant "NM" but schema supports other value types FUO -->
			<xsd:element name="OBX.2" type="nonNullString"/>
			<!-- Observation Identifier -->
			<xsd:element name="OBX.3">
				<xsd:complexType>
					<xsd:sequence>
						<!-- 09/17/2007 SGL Changed .1, .2 and .3 from CWE to CNE per 2_1 schema changes requested by MKM -->
						<!-- Loinc code for observation on patient" -->
						<xsd:element name="CWE.1" type="nonNullString" minOccurs="0"/>
						<!-- Loing short name" -->
						<xsd:element name="CWE.2" type="nonNullString" minOccurs="0"/>
						<!-- NAHLN Constant "LN" -->
						<xsd:element name="CWE.3" type="LN" minOccurs="0"/>
						<!-- Alternate codes not currently allowed here in Result message -->
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="Local" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- for use in multi-part observations -->
			<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
			<!-- Observation Value -->
			<xsd:element name="OBX.5">
				<!-- Our definition of ANY because this may support various types of observations about the patient -->
				<!-- mixed="true" allows for ST and NM types -->
				<xsd:complexType mixed="true">
					<xsd:sequence>
						<!-- Comparator: =, <, <=, >, >= -->
						<xsd:element name="SN.1" type="xsd:string" minOccurs="0"/>
						<!-- Number 1 -->
						<xsd:element name="SN.2" type="xsd:string" minOccurs="0"/>
						<!-- Separator: :, -, etc.-->
						<xsd:element name="SN.3" type="xsd:string" minOccurs="0"/>
						<!-- Number 2 -->
						<xsd:element name="SN.4" type="xsd:string" minOccurs="0"/>
						<!-- Code -->
						<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
						<!-- Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- Code System -->
						<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alt Text -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- Alt Code System -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original Text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Units if applicable -->
			<xsd:element name="OBX.6" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Units Code -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Units Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Abnormal Flags: Interpretation (See Implementation Guide for Codes) -->
			<xsd:element name="OBX.8" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<!-- NAHLN Constant "F" Only final results messaged -->
			<xsd:element name="OBX.11" minOccurs="1">
				<xsd:simpleType>
					<xsd:restriction base="nonNullString">
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ 
				This represents the physiologically significant time of the observation which for lab tests equals the 
				specimen collection date/time and is therefore redundant.	-->
			<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
			<!-- Method: Identifies specific NAHLN method version or for future use, local method -->
			<xsd:element name="OBX.17" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- OID for method -->
						<xsd:element name="CWE.1" type="OID" minOccurs="0"/>
						<!-- Text for method -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO" minOccurs="0"/>
						<!-- Alternate code for method -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alternate text for method -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- "L" for local code in most cases -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Date/Time of analysis (when test was set up) YYYYMMDDHHMMSS-ZZZZ -->
			<xsd:element name="OBX.19" minOccurs="0" type="CondDateTime"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="OPL_O37.SPECIMEN">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Specimen Segment -->
				<xsd:element ref="SPM"/>
				<!-- Zero or more observations on the specimen
				     Unlike the Result the SPECIMEN.OBX is not in a group -->
				<xsd:element name="OBX" type="SPECIMEN.OBX" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="OPL_O37.CONTAINER" minOccurs="0"/>
				<xsd:element ref="OPL_O37.OBSERVATION_REQUEST" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="SPM">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Specimen Identifier -->
				<xsd:element name="SPM.2">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Placer specimen ID -->
							<xsd:element name="EIP.1" minOccurs="1">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Placer Assigned specimen id for specimen or pool -->
										<xsd:element name="EI.1" minOccurs="1" maxOccurs="1">
											<xsd:simpleType>
												<xsd:restriction base="nonNullString">
												<xsd:maxLength value="32"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<!-- Text for placer ID numbering system -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Or Lab Constant either Lab OID or Lab OID for placer specimen IDs -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- If above is an OID, "ISO" otherwise omit -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Filler (Lab) assigned ID 
							     Used for referral tests to carry original lab's ID (debatable usage IMHO) -->
							<xsd:element name="EIP.2" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Lab Assigned specimen id for specimen or pool -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Text namespace ID -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Lab Constant either Lab OID or Lab OID for specimen IDs -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Type -->
				<xsd:element name="SPM.4">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen type-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Type Modifier -->
				<xsd:element name="SPM.5" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen type modifier -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"
								nillable="true"/>
							<!-- SNOMED text for specimen type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"
								nillable="true"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"
								nillable="true"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Additives -->
				<xsd:element name="SPM.6" minOccurs="0" maxOccurs="unbounded">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen additive-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen additive -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70371" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70371"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Source Site -->
				<xsd:element name="SPM.8" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen source site-->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for specimen source site -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Source Site Modifier -->
				<xsd:element name="SPM.9" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- SNOMED code for specimen source site modifier -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"
								nillable="true"/>
							<!-- SNOMED text for specimen source site -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"
								nillable="true"/>
							<!-- NAHLN Constant "SCT" -->
							<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"
								nillable="true"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Role: Patient sample, pool, grouped specimen, etc. -->
				<xsd:element name="SPM.11">
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN "P" Patient, "L" Pool, "G" Group -->
							<xsd:element name="CWE.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="P"/>
										<!-- Patient -->
										<xsd:enumeration value="L"/>
										<!-- Pool -->
										<xsd:enumeration value="G"/>
										<!-- Group -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- NAHLN "Patient", "Pooled", or "Group" -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70369" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70369"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Grouped Specimen Count -->
				<xsd:element name="SPM.13" type="xsd:int" minOccurs="0"/>
				<!-- Specimen Description -->
				<xsd:element name="SPM.14" type="xsd:string" minOccurs="0"/>
				<!-- Specimen Handling Code -->
				<xsd:element name="SPM.15" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- HL7 code for specimen handling code-->
							<xsd:element name="CWE.1" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="C37"/>
										<!-- Body temperature -->
										<xsd:enumeration value="AMB"/>
										<!--  Ambient Temperature-->
										<xsd:enumeration value="CAMB"/>
										<!-- Critical ambient temperature -->
										<xsd:enumeration value="REF"/>
										<!-- Refrigerated Temperature -->
										<xsd:enumeration value="CREF"/>
										<!-- Critical Refrigerated Temperature -->
										<xsd:enumeration value="FRZ"/>
										<!-- Frozen Temperature -->
										<xsd:enumeration value="CFRZ"/>
										<!-- Critical Frozen Temperature -->
										<xsd:enumeration value="DFRZ"/>
										<!-- Deep Frozen -->
										<xsd:enumeration value="UFRZ"/>
										<!-- Ultra Frozen -->
										<xsd:enumeration value="NTR"/>
										<!-- Liquid Nitrogen -->
										<xsd:enumeration value="PRTL"/> 
										<!-- Protect From Light -->
										<xsd:enumeration value="CATM"/>
										<!-- Protect From Air -->
										<xsd:enumeration value="DRY"/>
										<!-- Dry -->
										<xsd:enumeration value="PSO"/>
										<!-- No Shock -->
										<xsd:enumeration value="PSA"/>
										<!-- Do Not Shake -->
										<xsd:enumeration value="UPR"/>
										<!-- Upright -->
										<xsd:enumeration value="MTLF"/>
										<!-- Metal Free -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- text for handling code -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70396" -->
							<xsd:element name="CWE.3" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="HL70376"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Risk Code -->
				<xsd:element name="SPM.16" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- HL7 code for specimen risk code-->
							<xsd:element name="CWE.1" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="BIO"/>
										<!-- Biological -->
										<xsd:enumeration value="COR"/>
										<!-- Corrosive-->
										<xsd:enumeration value="ESC"/>
										<!-- Escape Risk -->
										<xsd:enumeration value="AGG"/>
										<!-- Aggressive -->
										<xsd:enumeration value="IFL"/>
										<!-- Material Danger Inflammable -->
										<xsd:enumeration value="EXP"/>
										<!-- Explosive -->
										<xsd:enumeration value="CFRZ"/>
										<!-- Material Danger Infectious -->
										<xsd:enumeration value="BHZ"/>
										<!-- Biohazard -->
										<xsd:enumeration value="INJ"/>
										<!-- Injury Hazard -->
										<xsd:enumeration value="POI"/>
										<!-- Poison -->
										<xsd:enumeration value="RAD"/> 
										<!-- Radioactive -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>							
							<!-- text for risk code -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70489" -->
							<xsd:element name="CWE.3" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="HL70489"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen Collection Date/Time: If available is the most important date/time for Epi -->
				<xsd:element name="SPM.17" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Start time used since collection is virtually instantaneous for our samples. YYYYMMDDHHMM-ZZZZ -->
							<xsd:element name="DR.1" type="nonNullString"/>
							<!-- For specimens collected over a defined period of time, e.g., 24 hour urine, this is the end
								time of the collection period and DR.1 is the start time. -->
							<xsd:element name="DR.2" type="nonNullString" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Specimen condition when sent -->
				<xsd:element name="SPM.24" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Code for specimen condition ( FROZ, COOL, etc, see Implementation Guide for Codes)-->
							<xsd:element name="CWE.1" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="AUT"/>
										<!-- Autolyzed -->
										<xsd:enumeration value="CLOT"/>
										<!-- Clotted -->
										<xsd:enumeration value="CON"/>
										<!-- Contaminated -->
										<xsd:enumeration value="COOL"/>
										<!-- Cool -->
										<xsd:enumeration value="FROZ"/>
										<!-- Frozen -->
										<xsd:enumeration value="HEM"/>
										<!-- Hemolyzed -->
										<xsd:enumeration value="LIVE"/>
										<!-- Live -->
										<xsd:enumeration value="ROOM"/>
										<!-- Room Temperature -->
										<xsd:enumeration value="SNR"/>
										<!-- Sample Not Received -->
										<xsd:enumeration value="WRM"/>
										<!-- Warm -->
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Text for specimen condition when sent -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "HL70493" -->
							<xsd:element name="CWE.3">
								<xsd:simpleType>
									<xsd:restriction base="nonNullString">
										<xsd:enumeration value="HL70493"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text (only component used for now) -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- Number of Specimen Containers -->
				<xsd:element name="SPM.26" type="xsd:int" minOccurs="0"/>
				<!-- Container Type 
				     Need to define coding system and value set -->
				<xsd:element name="SPM.27" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- ? code for container type once available -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- SNOMED text for container type -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "SCT" or other coding system once selected -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Local code -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Text: -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text (only component used for now) -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END SPM -->

	<xsd:complexType name="SPECIMEN.OBX">
		<xsd:sequence>
			<!-- NAHLN constant "NM" -->
			<xsd:element name="OBX.2" type="nonNullString"/>
			<xsd:element name="OBX.3">
				<xsd:complexType>
					<xsd:sequence>
						<!-- 09/17/2007 SGL Changed .1, .2 and .3 from CWE to CNE per 2_1 schema changes requested by MKM -->
						<!-- NAHLN Constant "42186-7" -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- NAHLN Constant "Number Spec Recd XXX Qn" -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "LN" -->
						<xsd:element name="CWE.3" type="LN"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Subid used for grouping, etc.  here? -->
			<xsd:element name="OBX.4" type="xsd:string" minOccurs="0"/>
			<!-- This number represents the number of specimens pooled to make up the identified specimen submitted in SPM -->
			<xsd:element name="OBX.5" type="xsd:int"/>
			<xsd:element name="OBX.6" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Units Code -->
						<xsd:element name="CWE.1" type="nonNullString"/>
						<!-- Units Text -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN constant "ISO+" but may be "ANS+" or "UCUM" at some point -->
						<xsd:element name="CWE.3" type="UNITS"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Abnormal Flags: Interpretation (See Implementation Guide for Codes) -->
			<xsd:element name="OBX.8" type="xsd:string" minOccurs="0" maxOccurs="1"/>
				<!-- NAHLN Constant "F" Only final results messaged -->
			<xsd:element name="OBX.11">
				<xsd:simpleType>
					<xsd:restriction base="nonNullString">
						<xsd:enumeration value="F"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Date/Time of observation YYYYMMDDHHMMSS-ZZZZ 
				This represents the physiologically significant time of the observation which for lab tests equals the 
				specimen collection date/time and is therefore redundant.	-->
			<xsd:element name="OBX.14" type="DateTime" minOccurs="0"/>
			<!-- Method: Identifies specific NAHLN method version or for future use, local method -->
			<xsd:element name="OBX.17" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<!-- OID for method -->
						<xsd:element name="CWE.1" type="OID" minOccurs="0"/>
						<!-- Text for method -->
						<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
						<!-- NAHLN Constant "ISO" -->
						<xsd:element name="CWE.3" type="ISO" minOccurs="0"/>
						<!-- Alternate code for method -->
						<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
						<!-- Alternate text for method -->
						<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
						<!-- "L" for local code in most cases -->
						<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
						<!-- Original text -->
						<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Date/Time of analysis (when test was set up) YYYYMMDDHHMMSS-ZZZZ -->
			<xsd:element name="OBX.19" minOccurs="0" type="CondDateTime"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="OPL_O37.CONTAINER">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="SAC">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="SAC.1" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- External Accession ID -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Namespace of assigner of External Accession ID if no OID -->
										<xsd:element name="EI.2" type="nonNullString" minOccurs="0"/>
										<!-- OID for program assigning External Accession ID-->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Container ID, Identifies the specific tube, bag, etc., that contains the specimen 
							     This will in most cases be a barcode -->
							<xsd:element name="SAC.3" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Container ID -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Namespace of assigner of Container ID if no OID -->
										<xsd:element name="EI.2" type="nonNullString" minOccurs="0"/>
										<!-- OID for program assigning External Accession ID-->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Carrier Type, Identifies the type of rack, case, etc., that contains the container -->
							<xsd:element name="SAC.9" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Code for carrier type (Source?) -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Code System -->
										<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
										<!-- Local code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Local Text: -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "L" -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Carrier ID Identifies the specific rack, case, etc., that contains the container -->
							<xsd:element name="SAC.10" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Container ID -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Namespace of assigner of Container ID if no OID -->
										<xsd:element name="EI.2" type="nonNullString" minOccurs="0"/>
										<!-- OID for program assigning External Accession ID-->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- CONTAINER.OBX will go here if needed -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- Each order on the same specimen forms a repeat of this group. -->
	<xsd:element name="OPL_O37.OBSERVATION_REQUEST">
		<xsd:complexType>
			<xsd:sequence>
				<!-- Additional order information (Once was Common to a set of orders, thus the name Order Common or ORC) 
				     Note that in the Order message the order of ORC and OBR is reversed making ORC required -->
				<xsd:element name="ORC" minOccurs="1"> <!-- Note: No longer optional -->
					<xsd:complexType>
						<xsd:sequence>
							<!-- NAHLN constant (for new orders at least)  "NW" 
								This field is more complicated than it looks.  The first electronic message with this order
								should be flagged NW for new.  Subsequent messages will be SC for status change.
								When sending results for which there is no corresponding electronic order message,
								this field is NW.  If responding to an electronic order (still call "unsolicited in HL7-speak)
								this will be SC.							-->
							<xsd:element name="ORC.1">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="NW"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<!-- Placer Group Number: Used for referral numbers, etc. -->
							<xsd:element name="ORC.4" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Referral Number -->
										<xsd:element name="EI.1" type="nonNullString"/>
										<!-- Text namespace ID -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Referral Number Assigner OID
											As a workable default for now assume all referral numbers
											have been issued by the NAHLN so the OID is 
											2.16.840.1.113883.3.5.1.8  -->
										<xsd:element name="EI.3" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "ISO" -->
										<xsd:element name="EI.4" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Date/Time of transaction YYYYMMDDHHMMSS 
								Ideally reflects the time the results were available to send, not the time sent
								System time stamp will work -->
							<xsd:element name="ORC.9" type="DateTime" minOccurs="0"/>
							<!-- Entering device: Used to record thing like MIM device used to 
							     record data for order -->
							<xsd:element name="ORC.18" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Code for device (Source?) -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Code System -->
										<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
										<!-- Local code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Local Text: -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "L" -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- General order information -->
				<xsd:element name="OBR">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Placer Order Number -->
							<xsd:element name="OBR.2" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Order number as received from field -->
										<xsd:element name="EI.1" type="nonNullString" minOccurs="1"/>
										<!-- Either text of placer order number system -->
										<xsd:element name="EI.2" type="xsd:string" minOccurs="0"/>
										<!-- Or OID for placer order number system -->
										<xsd:element name="EI.3" type="OID" minOccurs="0"/>
										<!-- If OID, this is "ISO" otherwise omitted -->
										<xsd:element name="EI.4" type="ISO" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Order Service Identifier -->
							<xsd:element name="OBR.4">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Loinc code for order if one exists -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Loinc short name -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "LN" -->
										<xsd:element name="CWE.3" type="LN" minOccurs="0"/>
										<!-- Local code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Local Text: -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- "L" in most cases -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original text  -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<!-- Reason for study.  Will probably replace CTI and OBX on visit at some point! -->
							<xsd:element name="OBR.31">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Code for Reason for Study (Specfic testing program) -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Test name of program -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- "L" for local code -->
										<xsd:element name="CWE.3" type="Local" minOccurs="0"/>
										<!-- Local code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Local Text: -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "L" -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original text  -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TQ1" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Priority as a SNOMED code -->
							<xsd:element name="TQ1.9">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Snomed Code for Priority -->
										<!-- Need to enumerate values for this! -->
										<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
										<!-- Text -->
										<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
										<!-- Code System -->
										<xsd:element name="CWE.3" type="SCT" minOccurs="0"/>
										<!-- Local code -->
										<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
										<!-- Local Text: -->
										<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
										<!-- NAHLN Constant "L" -->
										<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
										<!-- Original Text -->
										<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- END ORDER Group -->
	<!-- CTI is used here not for clinical trial info but to mimic the Reason for Visit OBX 
		 in the Result message.  -->
	<xsd:element name="CTI">
		<xsd:complexType>
			<xsd:sequence>
				<!-- "Sponsor study ID" used to carry OID of testing program. -->
				<xsd:element name="CTI.1">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="EI.1" type="xsd:string"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<!-- "Study Phase" used here to carry the equivalent of VISIT.OBX.5 in Result -->
				<xsd:element name="CTI.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- In normal usage CWE.1 - CWE.3 will be used and the rest not used
					the optionality is retained for future use and for unusual cases where
					the reson for visit is not anticipated by NAHLN codes. 
					This could need to be redefined as one of our ANY fields depending on future 
					uses of observation on the visit -->
							<!-- Result NAHLN code for reason for visit: S Surveillance, D Diagnostic, etc. -->
							<xsd:element name="CWE.1" type="xsd:string" minOccurs="0"/>
							<!-- Result Text: "Surveillance, etc." -->
							<xsd:element name="CWE.2" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.3" type="xsd:string" minOccurs="0"/>
							<!-- Local code for reason for visit: S Surveillance, D Diagnostic, etc. -->
							<xsd:element name="CWE.4" type="xsd:string" minOccurs="0"/>
							<!-- Local Result Text: "Surveillance, etc." -->
							<xsd:element name="CWE.5" type="xsd:string" minOccurs="0"/>
							<!-- NAHLN Constant "L" -->
							<xsd:element name="CWE.6" type="xsd:string" minOccurs="0"/>
							<!-- Original text of reason for visit -->
							<xsd:element name="CWE.9" type="xsd:string" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
