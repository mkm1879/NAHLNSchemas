				<xsd:element name="GT1.2" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<!-- Guarantor ID -->
							<xsd:element name="CX.1" type="xsd:string"/>
							<!-- Assigning authority either text (HD.1) or preferred OID (HD.2,3) -->
							<xsd:element name="CX.4">
								<xsd:complexType>
									<xsd:sequence>
										<!-- Text value -->
										<xsd:element name="HD.1" type="xsd:string" minOccurs="0"/>
										<!-- OR OID plus "ISO" -->
										<xsd:element name="HD.2" type="OID" minOccurs="0"/>
										<xsd:element name="HD.3" type="ISO" minOccurs="0"/>
